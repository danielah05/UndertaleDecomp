	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1126", "\\E3あの　バカな王さま^1と&くるひも　くるひ^1も&なにか　かんじようと&どりょく　してみた/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1127", "\\E1でも　そのうち&たえられなくなって…/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1128", "ボクは　いえを　でた/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1129", "そして^1　いせきに&たどりついて…/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1130", "\\E0そこで^1　あの　“おばさん”を&みつけたんだ/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1131", "このひとなら　きっと&ボクの　こころを&とりもどして　くれるって&おもった/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1132", "\\E1…/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1133", "\\E7でも　ダメだった/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1134", "\\E6はは…/%%")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1144", "\\E1あの　ふたりは&やくたたず　だったのさ/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1145", "そう　きづいて　ボクは&ほんとうに　がっかりしたよ/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1146", "ボクは　だれかを&あいしたかっただけ…/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1147", "だれかの　ことを　たいせつに&おもいたかっただけ/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1148", "\\[C]^1　キミは&しんじて　くれないかも&しれないけど…/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1149", "\\E2これじゃ　もう^1…&いきてるイミなんてないな&って　おもったんだ/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1150", "\\E7あいのない　せかいなんて…/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1151", "\\E6キミのいない　せかいなんて…&イミがない/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1152", "\\E1だからね…/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1153", "\\E0ボクも　キミの　まねを&することに　したんだ/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1154", "\\E1じぶんの　そんざいを&けしてしまう　ことにした/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1155", "\\E1そして…/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1156", "\\E0ボクは　それを^1　じっさいに&やってのけたんだよ/%%")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1165", "\\E1なのに… このよに　わかれを&つげようとした　しゅんかん…/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1166", "とつぜん　ふあんが&こみあげてきた/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1167", "\\E9タマシイを　もたない&そんざいは^1…　しぬと&どうなるんだろう…って/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1168", "\\E1そのとき　ボクの　なかで&“ほんのう”のような&なにかが　あばれだした/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1169", "「いやだ^1…　ボクは&しにたくない！」　って/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1170", "…/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1171", "そして　めが　さめた/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1172", "なにもかも　わるいゆめ&だったみたいに…/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1173", "ボクは　もとの　にわに　いた/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1174", "\\E0じぶんの　「セーブポイント」に&もどってたんだ/%%")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1183", "そこで　ボクは^1 &じっけんを　してみる&ことにした/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1184", "なんども　なんど^1も　じぶんを&しぬ　ちょくぜんまで&おいこんでみた/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1185", "そのまま　せかいから&りだつ　してしまうことは&いつでも　できた…/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1186", "でも…　「いきたい」という&ケツイさえ　もっていれば…/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1187", "\\E0ボクは　なんどでも&ふっかつ　できた/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1188", "\\E6すごいだろ^1 &\\[C]…&/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1189", "\\E0ボクも　すごいと　おもったよ/%%")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1197", "\\E1はじめ^1は　このちからを&いいことに　つかったよ/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1198", "\\E0みんなと　「ともだち」に&なって…/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1199", "みんなの　なやみを&カンペキに　かいけつして&あげた/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1200", "みんなと　なかよくするのは&ゆかいだったよ/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1201", "\\E1…はじめのうちは…　ね/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1202", "\\E1だけど　なんども&くりかえしてる　うちに^1 &さきが　よめるように&なってきた/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1203", "「これを　あげたら&こいつは　なんていうかな」…/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1204", "「ボクが　こういったら&こいつらは　どうするかな」…/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1205", "こたえが　わかってしまった^1ら&それで　おしまいさ/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1206", "それいじょうは　なにもない/%%")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1217", "\\E1…すべては　こうきしんから&はじめたことだった/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1218", "\\E0こいつらを　ころしてみたら&どうなるんだろう…　ってね/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1219", "\\E2「やりたくて　やるわけじゃ&ないんだ」って　じぶんに&いいきかせたよ/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1220", "\\E1「たしかめるために&しかたなく　やるんだ」ってね/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1221", "\\E8ハハハ^1…　でも&そんなのは　いいわけさ！/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1222", "\\E9スカッとして　きぶんが&いいってこと…　キミは&だれよりも　よくしってる&はずだ/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1223", "\\E8ま^1　すくなくとも&じぶんで　やらずに&みてるだけの　ヤツらよりは&マシさ…/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1224", "\\E9どうなるのか　しりたい&クセに^1　じぶんでは&できない　なさけない&れんちゅう…/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1225", "\\E0いまも　きっ^1と&そんなヤツらが　ボクたちを&みてるんじゃ　ないかな/%%")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1235", "\\E1さいきんじゃ^1　それすらも&タイクツに　なってきた/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1236", "\\E7ねえ　\\[C]^1 &わかるかい？/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1237", "\\E6ボクは　このせかいの&なにもかもを　しりつくして&しまったんだ/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1238", "\\E1すべての　ほんを　よんで^1 &すべての　ほんを　もやした/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1239", "ミニゲームを　ぜんぶ&クリアして^1　そして　ぜんぶ&しっぱいも　してみた/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1240", "みんなの　ねがいを　きいて^1 &そして　みんなを　ころした/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1241", "すうじも^1…　セリフも^1…&みたことのない　ものは&ひとつもない/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1242", "\\E1だけど　キミは^1…&キミだけは　ちがった/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1243", "\\E0\\[C]^1 &キミの　こうどうだけは&まったく　よめなかった/%%")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1252", "\\E1いせきで　キミを　みつけた&ときは^1…　まだ　だれなのか&きづいて　いなかったよ/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1253", "\\E8だから　おどして　タマシイを&とってやろうと　おもったんだ/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1254", "\\E1でも　しっぱい　しちゃった/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1255", "\\E0だから　セーブファイルを&ロードしようと　したのに…/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1256", "\\E1…できなかった/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1257", "\\E9\\[C]^1…&キミのケツイは…/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1258", "\\E8どういうわけ^1か&ボクのケツイより&つよいみたいだね！/%%")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1267", "\\E1\\[C]^1 &キミに　ひとつだけ&ききたいことが　ある/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1268", "ここから　どうやって&いせきに　もどったの？/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1269", "\\E0…まっ^1て　…こうじゃない？/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1270", "“おばさん”が　でていった&とき^1　なきがらを&もっていったんでしょ？/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1271", "\\E1そして　おはかに&うめたんだ…/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1272", "\\E0だから　キミは　ちかしつに&とじこめられた　ままに&ならずに　すんだんだね/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1273", "\\E1…でも^1　それなら　どうして…/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1274", "キミは　めを　さましたの？/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1275", "\\E6…ボクの　よぶこえが&きこえたとか…？/%%")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1286", "\\E1ま^1　そんなことは&どうでもいいさ/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1287", "\\E1\\[C]^1　ボクは　もう&うんざりなんだ/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1288", "だれに　あっても…/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1289", "どこへ　いっても…&タイクツで　しかたない/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1290", "これいじょう&はなのすがたで&いることにも&たえられないよ/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1291", "\\[C]/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1292", "\\E0でも^1　ひとつだけ&やっておきたい　ことがある/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1293", "キミと　ボクとで&はじめたことを&きちんと　おわらせよう/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1294", "みんなを　じゆうに　するのさ/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1295", "\\E8やつらに　おもいしらせて&やろう^1よ　ニンゲンの&ほんしょうをさ！/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1296", "\\E6けっきょく…/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1297", "\\E9このよは　「ころすか&ころされるか」　だってね！/%%")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1307", "\\E1そのあと…？/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1308", "\\E7そうだな…/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1309", "\\E9このちからの　つかいみちは&いくつか　かんがえてあったよ/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1310", "\\E8フフフ…/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1311", "\\E1…/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1312", "\\E1でも^1　ここで　キミと　あって&きが　かわった/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1313", "\\E6\\[C]^1…&キミが　いてくれたら…/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1314", "\\E7ちじょうで　へいぼんに&くらす　っていうのも&わるくなさそうだ/%%")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1324", "\\E0それに　こんかいは　もう&そとへ　ちょうたつ　しにいく&ひつようも　ないしね…/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1325", "タマシイは　もう^1 &6つ　てに　はいってる^1…&王さまが　どこかに&かくしてるけどね/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1326", "\\E1ボクも　あのて　このてで&かくしばしょを　ききだそうと&したけど…/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1327", "\\E3アイ^1ツ　どうしても&おしえて　くれないんだ/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1328", "\\E1でも　\\[C]…/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1329", "\\E0キミが　たのめば&きっと　おしえてくれるよ/%%")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1339", "\\E0どうして　キミに&こんなことを　はなすのか&って？/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1340", "\\E6\\[C]^1 &いったじゃないか…/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1341", "\\E1あれから　ずいぶん&たったけど…/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1342", "\\E0ボクの　ことを　ほんとうに&わかって　くれてるのは&いまでも　キミだけ　なんだよ/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1343", "\\E8キミは　ボクに　くだらない&どうじょうなんて　しない&からね/%%")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1353", "\\E9ボクたち　みたいな&イキモノは…/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1354", "\\E0あいてが　じゃまに　なったら&ためらいなく　ころせる/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1355", "\\E0だから…/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1356", "\\E1だ^1…　だから…　ね…/%%")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1383", "\\E2…/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1384", "\\E2… ハ^1…　ハハ…/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1385", "\\E6…なんだろう^1…&…このキモチ…/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1386", "ボク^1…　どうして&ふるえてるのかな？/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1387", "…/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1388", "あのさ^1…　\\[C]^1…&このまえのこと&おこってないよね？/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1389", "…/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1390", "\\E4え^1…　ちょっ^1…　ちょっと&まってよ！/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1391", "…く^1…　くるな！！/%%")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1412", "や^1…　やっぱり&きが　かわった…/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1413", "これは　あんまり&いいアイデアじゃ&なさそうだな/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1414", "\\[C]^1　キミは&もどったほうが　いい/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1415", "\\E0このせかいは&いまの　ままでも&ぜんぜん　わるくないよ！/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1416", "…/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1417", "\\E4お　お　お…　おい！&その　ブキミな　かお&やめろって！/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1418", "わらえないよ！/")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_1419", "アクシュミだぞ！！/%%")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_295", "むかしむかし^1　いせきに&ひとりの　ニンゲンが&おちてきました/*")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_307", "ケガをした　ニンゲンは&おおきなこえで&たすけを　よびました/%%")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_333", "そのこえを&ききつけたのは^1 &王さまの　むすこ^1 &アズリエルでした/*")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_345", "アズリエルは&ニンゲンを　おしろへ&つれてかえりました/%%")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_370", "やがて^1　アズリエルと&ニンゲンは&きょうだいの　ように&なかよくなりました/*")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_383", "王さまと　女王さまは&ニンゲンを&わがこの　ように&かわいがりました/*")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_395", "ちていのせかいは&きぼうに&つつまれました/%%")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_421", "ところが^1…&あるひ…/*")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_433", "ニンゲンが&おもい　びょうきに&かかりました/%%")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_459", "ニンゲンは&さいごに　ひとつだけ&ねがいを　かなえて&ほしいと　いいました/*")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_472", "ふるさとの　むらの&はなばたけを&ひとめ　みたいと…/*")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_484", "…しかし&わたしたちには&どうすることも&できませんでした/%%")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_511", "そして…/*")
	ds_map_add(global.text_data_ja, "obj_storybattle_gen_523", "その　よくじつ…/%%")
