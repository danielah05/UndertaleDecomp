	ds_map_add(global.text_data_ja, "SCR_TEXT_2047", "…そうか/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2048", "なら、&オレさまも&みずからの&しっぱいを&みとめ&よう…/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2058", "＊ いらっしゃいませ^1！&＊ こうきゅうホテル「スノーテル」へ&　 ようこそ。/")
