	ds_map_add(global.text_data_ja, "SCR_TEXT_2694", "＊ (からっぽだ)/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2698", "＊ (うちゅうしょくを　ひろう？)& & \\>0はい\\>1いいえ\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2699", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_270", "  %%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2704", "＊ (クーラーボックスだ^1)&＊ (メーカーのなまえは　ない。&　 まるで　しんぴんのようだ…)/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2705", "＊ (なかには　フリーズドライの&　 うちゅうしょくが　いくつか&　 はいっている)/")
