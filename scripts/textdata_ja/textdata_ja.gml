	ds_map_add(global.text_data_ja, "scr_papcall_879", "\\E0ほら^1！&もう&かなった！/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_885", "きさまは&なにを&おねがい&するのッ？/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_891", "＊ そこには^1　まいにち&　 だれかがきて^1　ほしに&　 ねがいごとをする…/")
	ds_map_add(global.text_data_ja, "scr_papcall_892", "\\E1＊ わたしに　まかせろッ！/")
	ds_map_add(global.text_data_ja, "scr_papcall_893", "＊ みんなの　ねがいを&　 かなえてやるッ！/")
	ds_map_add(global.text_data_ja, "scr_papcall_895", "あしが&八ほんに&なります&ように…/")
	ds_map_add(global.text_data_ja, "scr_papcall_896", "\\E0そしたら、&イカした&たんパンを&四まい&はける&からッ！/")
	ds_map_add(global.text_data_ja, "scr_papcall_898", "＊ …ただし　いちぶ&　 れいがいも　あるッ！/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_903", "＊ なんだ^1？　おまえも&　 ねがいごとが　あるのか？/")
	ds_map_add(global.text_data_ja, "scr_papcall_904", "\\E3＊ じゃあ^1　そこの&　 おねがいボックスに&　 いれておけ。/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_915", "そこの&かべには、&むかしの&もじが&たくさん&かいてある/")
