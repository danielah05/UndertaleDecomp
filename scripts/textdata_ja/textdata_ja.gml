	ds_map_add(global.text_data_ja, "SCR_TEXT_994", "よし^1！&では、&さいごに&だいじな&ことを、&おしえよう/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_995", "この&パズルは…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_996", "かんぜん^1に&ランダムに&せいせい&されるッ！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_997", "オレさまが&この&スイッチを&おせば…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_998", "だれも&みたことの&ない&パズルが&たんじょう&するのだ！/")
