	ds_map_add(global.text_data_ja, "SCR_TEXT_2062", "＊ おもどりですね^1？&＊ おへやで　すきなだけ&　 おやすみ　いただけますよ。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2063", "＊ どうなさいます？& & \\>0とまる\\>1やめる\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2065", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2069", "＊ え^1？&＊ カギを　もう1つ　かしてほしい^1？&　 そんなの　ダメにきまってるでしょ！")
