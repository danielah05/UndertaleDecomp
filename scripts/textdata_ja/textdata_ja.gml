	ds_map_add(global.text_data_ja, "SCR_TEXT_2219", "\\TS \\F0 \\T0 %")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2220", "＊ ツー…/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2227", "＊ (オーブンには　からっぽの&　 パイざらが　はいっている)/%%")
