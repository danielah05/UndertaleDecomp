	ds_map_add(global.text_data_ja, "scr_papcall_922", "もしもし！/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_928", "＊ そこの　かべには&　 モンスターの　ひげきの&　 れきしが　きざまれてる…/")
