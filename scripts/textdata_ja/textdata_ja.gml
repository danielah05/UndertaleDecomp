	ds_map_add(global.text_data_ja, "SCR_TEXT_5499", " %%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_55", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5506", "＊ (かいちゅうでんとうの　でんちが&　 きれている)/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5510", " %%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5517", "＊ やあ　キミ^1！&＊ オレのなまえ　おぼえてる？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5518", "＊ (おぼえてる？)& & \\>0はい\\>1いいえ\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5519", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5527", "＊ えっ…　えええぇぇえっ！^1？&＊ ホントに　おぼえてたの！？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5528", "＊ まさか^1…　このオレが^1…&　 まけるなんてええええぇぇ！/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5533", "＊ えっ…　えええぇぇえっ！^1？&＊ おぼえてないの！？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5534", "＊ まさか^1…　このオレが^1…&　 まけるなんてええええぇぇ！/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_554", "＊ ボクのちょうしは　どうですか^1？&＊ このまえあげた　ボクのからだの&　 かけらのことですけど…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5541", "＊ (しろい　こいぬだ^1)&＊ (ぐっすり　ねむっている…)/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5542", "＊ (イヌと　たたかう？)& & \\>0はい\\>1いいえ\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5543", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5550", "＊ (イヌとは　たたかえない)/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5551", "＊ (イヌのしたに　しいてある　ぬのは&　 あちこち　あなだらけのようだ)/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5552", "＊ (イヌは　ぬのに&　 「パッチ」を　あてないと&　 いけないようだ)/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5553", "＊ (それが　すんだら　たたかえる^1)&＊ (…かも？)/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5554", "＊ (よくみると^1　ぬのに　あいた&　 あなは　どんどん　おおきく&　 なっているようだ)/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5555", "＊ (イヌが　ぜんぶの　あなに&　 「パッチ」をあてるには&　 かなりのじかんが　かかりそうだ)/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5556", "＊ (イヌは　ふつ^1う&　 あみものが　とくいではない)/")
