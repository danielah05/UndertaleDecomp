	ds_map_add(global.text_data_ja, "obj_endflowey_332", "\\E5もどって　かれらと&“あそんで”みたら&どうかな？/")
	ds_map_add(global.text_data_ja, "obj_endflowey_333", "\\E4だって　ゆうじょうの&パワーは　いつだって…/")
	ds_map_add(global.text_data_ja, "obj_endflowey_334", "しあわせのカギ^1…&でしょ？/%%")
	ds_map_add(global.text_data_ja, "obj_endflowey_349", "\\E0へえ^1。キミは　ころしても&かまわないと　おもってるんだ&ね…？/")
	ds_map_add(global.text_data_ja, "obj_endflowey_350", "\\E4そっか。/")
	ds_map_add(global.text_data_ja, "obj_endflowey_351", "じゃあ　キミとボク^1は&いのちの　とらえかたが&おんなじ　みたいだね。/")
	ds_map_add(global.text_data_ja, "obj_endflowey_352", "キミを　みていて&すごく　べんきょうに&なったよ。/")
