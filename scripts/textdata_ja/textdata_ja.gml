	ds_map_add(global.text_data_ja, "SCR_TEXT_1562", "＊ ちょぉっと　まったぁ^2！&＊ オイラを　おして　どかそうったって&　 そうは　とんやが　おろさねェやぃ。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1563", "＊ あぁん^2？&＊ 「おねがいですから&　 うごいてください」　だぁ？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1564", "＊ ったく^1　しょーがねェなぁ^1…&　 こんかいだけ　とくべつだぜ？/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1569", "＊ あぁあ^2？&＊ もっと　うごけだぁ？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_157", "＊ (ときどき^1　ゴミも　いっしょに&　 ながれてきて…)/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1570", "＊ ほらよ^1　これで　どうだぃ。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1575", "＊ ああぁ^2？&＊ うえじゃなくて&　 よこに　うごけだぁ？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1576", "＊ わーったよ^1。&＊ こーいうことだろ？/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1579", "＊ どーいたしまして。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_158", "＊ (…どこまでも　ふかい&　 たきつぼの　やみに　きえていく)/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1583", "＊ あああぁ^2？&＊ じーっとしてろ　だぁ！？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1584", "＊ ったく^1…　なんども&　 いったりきたり　させやがって…/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1589", "＊ だったら　さいしょっから&　 そう　いっといてくれよな！？/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_159", "＊ (なんのやくにも　たたない&　 ゴミのサイクルを　えんえんと&　 みていたら…)/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1592", "＊ バリアが　きえったってェ^1？&＊ んじゃあ　オイラも　いくかな…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1593", "＊ アンタ^1　わりィが　ちょいと&　 オイラを　おしてくれるかい？/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_160", "＊ (ケツイが　みなぎった)/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1603", "＊ 「おひとつ　どうぞ」&＊ キャンディを　1つ　もらう？& \\>0はい\\>1いいえ\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1604", "＊ 「おひとつ　どうぞ」と　かいてある。&＊ キャンディを　1つ　もらう？& \\>0はい\\>1いいえ\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1606", "＊ よくばると　いいことはない。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1607", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_161", "＊ (なんの　やくにも　たたない&　 ゴミに　まみれていると&　 ケツイが　みなぎった)/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1624", "＊ キャンディを　1つ　てにいれた。&＊ (\\*Cをおすと　メニューがひらく)/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1625", "＊ キャンディを　もう1つ　とった^1。&＊ なんと　おぎょうぎの　わるい…！/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1626", "＊ キャンディを　もう1つ　とった。&＊ サイテーなクズに　なったきぶんだ…/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1628", "＊ いちどに　たくさん　とりすぎた。&＊ キャンディが　ゆかにこぼれた。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1631", "＊ この　じごくのような　せかいで^1は&　 とっていい　キャンディは&　 3つだけ…/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1635", "＊ キャンディを1つ&　 もらおうとした^1…&　 が^1　これいじょう　もてない。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1639", "＊ もらわない　ことにした。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_164", "＊ (おだやかな　しずけさのなか^1で&　 ケツイが　みなぎった…)/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1644", "＊ グーグーグー^1…&＊ グーグーグー…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1645", "＊ グーグーグー^1…&＊ (まだ　いるノ…？^1)&＊ グーグーグー…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1646", "＊ (この　おばけは&　 「グーグー」と　いいつづけて^1 &　 ねたふりを　している)/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1647", "＊ ムリヤリ　どかす？& & \\>0はい\\>1いいえ\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1648", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_165", "＊ (ふしぎな　なにかを^1…　かんじる)&＊ (ｹッィ　みなぎﾀ！)/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1675", "* It says 'Take them all.'&* Take a candy?&         Yes         No \\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1676", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_168", "＊ (くろい　かぜが　ないている^1。&＊ ケツイが　みなぎった)/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_169", "＊ (かぜの　なきごえが&　 おだやかに　なった^1。&＊ ケツイが　みなぎった…)/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1692", "* You took a piece.&* Boy^1, that's heavy./%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1694", "* You can't carry more.&* It's just too heavy./%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1695", "* You tried to take a piece&  of candy^1, but you didn't&  have any room./%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1698", "* You decided not to take some./%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_170", "＊ (かぜが　なきやんだ^1。&＊ ケツイが　みなぎった…)/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1705", "＊ クモのすに　7G　のせる？& & \\>0はい\\>1いいえ\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1706", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1723", "＊ クモたちが　おりてきて&　 ドーナツを　くれた。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1724", "＊ おかねが　たりなかった。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1726", "＊ これいじょう　アイテムを&　 もてない。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1729", "＊%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1736", "＊ クモのすに　18G　のせる？& & \\>0はい\\>1いいえ\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1737", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_174", "＊ (こんなところに　あやしげな&　 ラボがある…)/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_175", "＊ (それをみて&　 ケツイが　みなぎった)/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1754", "＊ クモたちが　おりてきて&　 ジュースのビンを　くれた。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1755", "＊ おかねが　たりなかった。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1757", "＊ これいじょう　アイテムを&　 もてない。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1760", "＊%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1771", "＊ いろあせたリボンを　みつけた。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1772", "＊ これいじょう　もてない。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1778", "＊ スイッチだ。&＊ おしてみる？& \\>0はい\\>1いいえ\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1779", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1788", "＊ なにも　おこらなかった。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1789", "%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1790", "＊ しつこくされて　スイッチは&　 とても　きまずそうだ。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1796", "＊ スイッチだ。&＊ おしてみる？& \\>0はい\\>1いいえ\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1797", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_180", "＊ (ふきだす　じょうきと　はぐるまの&　 おとをきいて^1…&　 ケツイが　みなぎった)/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1806", "＊ どこかで　カチッと　おとがした。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1807", "%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1817", "＊ おもちゃのナイフを　みつけた。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1818", "＊ これいじょう　もてない。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1824", "＊ トリエルの　にっきだ。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1825", "＊ まるが　ついているところを&　 よみますか？&\\>0はい\\>1いいえ\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1830", "＊ にっきを　よんだ…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1831", "＊ 「スケルトンは　どうして&　 ともだちが　ほしかった？」/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1832", "＊ 「さみしさが　ホネみに&　 しみたから…」/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1833", "＊ ほかにも　にたような　ダジャレが&　 たくさん　かきつらねてある。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1835", "%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_184", "＊ (とおくに　ブキミな　たてものが&　 みえる…)/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1842", "＊ バタースコッチ　シナモンパイが&　 1きれ　おいてある。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1843", "＊ これいじょう　もてない。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_185", "＊ (ケツイが　みなぎった)/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1853", "＊ カタツムリパイが　1きれ&　 おいてある…/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1854", "＊ これいじょう　もてない。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1859", "＊ あら^1　いらっしゃい！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1860", "＊ パイが　さめるまで&　 もうすこし　まってね。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1861", "＊ おひるねでも　したら？& & \\>0はい\\>1いいえ\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1862", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1867", "＊ おやすみなさい。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_187", "＊ (いつかネズミは　コンピューターの&　 きんこを　ハッキングして&　 チーズをたべるかもしれない)/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1870", "\\E1＊ それじゃ　わたしと&　 おしゃべりする？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1871", "＊ あら^1　もう　おきたの？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1872", "\\E0＊ …あたらしい　かぞくが&　 できて　とても&　 うれしいわ。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1873", "＊ あなたに　よませて&　 あげたい　ふるいほんが&　 たくさんあるの。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1874", "＊ とっておきの　ムシとり&　 スポットにも　つれてって&　 あげましょうね。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1875", "＊ それから^1　おべんきょうは&　 わたしが　みてあげます&　 からね。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1876", "＊ いがいかも&　 しれないけれど…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1877", "＊ じつは　がっこうの&　 せんせいに　なるのが&　 ゆめだったのよ。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1878", "\\E1＊ …そんなに　いがいでも&　 なかったかしら。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1879", "\\E5＊ とにかく！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_188", "＊ (そうおもうと&　 ケツイが　みなぎった)/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1880", "\\E0＊ あなたと　ここでくらせて&　 とてもうれしいわ。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1881", "\\E1＊ ん…^1？&　 なにか　ごようかしら？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1882", "＊ どうしたの？&\\>0どうも\\>1いつ　うちに&\\>0しない\\>1かえれる？\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1883", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1891", "＊ そう^1　なにか　ようが&　 あるときは　こえをかけて&　 ちょうだいね。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1895", "\\E1＊ え^1？　なにをいってるの^1…&＊ きょうから　ここが&　 あなたの　おうちよ。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1896", "＊ それより^1…　いまよんでる&　 このほんの　おはなしを&　 しましょうか？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1897", "\\E0＊ あら^1！　いらっしゃい。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1898", "＊ いまよんでる&　 このほんの　おはなしを&　 してあげましょうか？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1899", "\\E0＊ 『カタツムリ：72の&　 つかいみち』っていうの。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1900", "＊ ききたい？& \\>1いせきから& \\>0うん\\>1でたい\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1901", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1907", "＊ それじゃ　カタツムリの&　 マメちしきを　1つ…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1908", "\\E1＊ それじゃ^1…^1…^1…&＊\\E0 カタツムリの　マメちしきを&　 おしえて　あげましょうか？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1909", "\\E2＊ カタツムリはね…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_191", "＊ (きたに　きょだいな&　 たてものが　みえる^1)&＊ (ケツイが　みなぎった)/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1911", "\\E0＊ 舌が　チェーンソーみたいに&　 ギザギザなの。&＊ 「歯舌」っていうのよ。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1912", "\\E0＊ せいちょうすると&　 おしりの　いちが　180ど&　 かわることも　あるのよ？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1913", "\\E0＊ クツひもの　そざいには&　 むいていないのよ？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1914", "\\E0＊ しゃべるのよ^2？&　 とって^2も　ゆっくりね^2。&＊ なぁんて^1　ウソでした。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1915", "\\E0＊ おもしろいでしょう？& \\>1いせきから& \\>0うん\\>1でたい\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1916", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1921", "＊ それじゃ^1　また　ようが&　 あるときは　こえをかけて&　 ちょうだいね。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1924", "\\E1＊ …わたしは　ちょっと&　 ようじが　あるから^1…&＊ ここで　まっていなさい。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1927", "\\E0＊ どうしたの？& \\>1いせきから& \\>0どうもしない\\>1でたい\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1930", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1934", "＊ ようが　あるときは^1 &　 こえを　かけて&　 ちょうだいね。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1935", "\\E1＊ …わたしは　ちょっと&　 ようじが　あるから^1…&＊ ここで　まっていなさい。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1936", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1940", "\\E2にょほほ&ほほ…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1941", "きさまの&ように&ひんじゃく&な&ものすら、&たおすこと&が&できない&とは…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1942", "アンダイン&に&あきれ&られて&しまうな…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1943", "オレさまは&ロイヤル・&ガードに&なんて&なれない&し…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1944", "ともだちの&にんずう&だって、&ふえは&しない&だろうッ！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1945", "\\TS \\F0 \\T0 %")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1946", "＊ (なんと　いう？)& \\>0ともだちに\\>1ダサい& \\>0なろう\\>1やつめ\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1947", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_195", "＊ (ホテルの　おちついた&　 ふんいきに^1…&　 ケツイが　みなぎった)/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1956", "\\E0やった^1！&やったぞ！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1957", "きさまは、&ぼうりょく&を&ふるわな&かった！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1958", "\\E5しょうじき&ちょっと^1、&こわかった&よ…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1959", "\\E0でも、&きさまは&りっぱな&ひとに&なりつつ&ある！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1960", "\\E7オレさま、&うれしくて&ないちゃい&そうッ！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1961", "\\E3…あれ^1？&そういえば&オレさま、&きさまを&つかまえる&んじゃ&なかった&っけ…？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1962", "\\E0…^1ま&いっか！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1963", "きさまには&このまま、&もっと&もっと&りっぱな&ひとに&なって&もらいたい&からねッ！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1964", "だから、&ぜーんぶ&みずに&ながそう！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1965", "ついでに、&この&せかいから&だっしゅつ&する&ほうほうも&おしえ&ちゃうッ！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1966", "このまま&ずーっと&ひがしへ&すすんで！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1967", "そうすると&王さまの&おしろに&つくよ/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1968", "そこから、&ちじょうに&でられる&からねッ！/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1978", "えッ^1！&ホント&にッ^1？&オレさま&と、&おともだち&に、&なって&くれる&のッ？/")
