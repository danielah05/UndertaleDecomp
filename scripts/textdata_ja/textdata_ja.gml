	ds_map_add(global.text_data_ja, "obj_endflowey_354", "\\E8キミが　ころした&にんずうは…/")
	ds_map_add(global.text_data_ja, "obj_endflowey_355", "\\E9しょうじき　ショボいよ。/")
	ds_map_add(global.text_data_ja, "obj_endflowey_356", "\\E8バカだね。/")
