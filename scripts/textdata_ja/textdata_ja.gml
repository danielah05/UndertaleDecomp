	ds_map_add(global.text_data_ja, "SCR_TEXT_7020", "\\TS \\F0 \\T0 %")
	ds_map_add(global.text_data_ja, "SCR_TEXT_7021", "＊ ツー…/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_7025", "＊ プルルルル…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_7026", "＊ … /")
	ds_map_add(global.text_data_ja, "SCR_TEXT_7027", "＊ (どこかで　でんぱが　イヌに&　 ぶつかり^1　あいてに&　 とどかなかった)/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_704", "＊ いらっしゃい^1！&＊ 「ナイスクリーム」&　 おひとつ　いかが？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_7040", "＊ (だれも　でない^1)&＊ (でんわの　でんちが&　 きれているのかも　しれない)/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_705", "＊ 舌にヒンヤリ^1　ココロはホッコ^1リ&　 おいしいフローズンスイーツだよ！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_7052", "＊ (「はこのなか&　 イヌのけ　いっぱい&　 つまってる」　　―じあまり)/%%")
