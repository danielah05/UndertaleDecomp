	ds_map_add(global.text_data_ja, "SCR_TEXT_2676", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2682", "＊ (れいぞうこを　あけた^1)&＊ (くさったような　においが&　 ひろがる…)/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2683", "＊ (なかの　たべものは&　 ずいぶんまえに&　 くさってしまったようだ)/%%")
