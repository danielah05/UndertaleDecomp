	ds_map_add(global.text_data_ja, "obj_endflowey_327", "\\E6キミは　ボクの　たのみを&きいてくれたから…/")
	ds_map_add(global.text_data_ja, "obj_endflowey_328", "\\E5ほんとうの&「ハッピーエンド」を&みせてあげてもいいか…/")
	ds_map_add(global.text_data_ja, "obj_endflowey_329", "\\E0だけど^1　それには&キミひとりだけの&ちからじゃ　ムリだ。/")
	ds_map_add(global.text_data_ja, "obj_endflowey_330", "パピルス^1　アンダイ^1ン&そして　アルフィー…/")
