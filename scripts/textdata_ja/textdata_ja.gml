	ds_map_add(global.text_data_ja, "scr_papcall_843", "＊ そのパズルの　つちの&　 タイルは^1　アルフィーが&　 ならべてくれたんだ。/")
	ds_map_add(global.text_data_ja, "scr_papcall_844", "＊ たいへんだった^1が&　 そのあと　すなのおしろを&　 つくって　あそんだ。/")
	ds_map_add(global.text_data_ja, "scr_papcall_845", "\\E9＊ わたしたちは　いい&　 コンビだ^1。アルフィーが&　 おしろを　つくって…/")
	ds_map_add(global.text_data_ja, "scr_papcall_846", "\\E6＊ わたしは　それを&　 ブッこわす！/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_85", "そこは、&ワンボーが&けいびを&たんとう&してる/")
	ds_map_add(global.text_data_ja, "scr_papcall_851", "＊ いまじゃ　おしろは&　 あとかたも　ないけどな…/%%")
