	ds_map_add(global.text_data_ja, "SCR_TEXT_862", "＊ ま^1　きにすんなって。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_863", "\\E1＊ オイラだって&　 「フライドスノー」なんか&　 もってないしさ。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_867", "＊ アンタ^1　かねもち&　 なんだな。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_868", "\\E3＊ わるいけど…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_869", "\\E3＊ このフライドスノーを&　 てばなす　つもりは&　 ないぜ。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_870", "\\E2＊ オイラの　たいせつな&　 おもいでが　たっぷり&　 つまってるからさ。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_875", "＊ こんなに　おかいどく&　 なのに？/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_880", "なにィッ^1！&オレさまの&ワナを、&どうやって&かいひ&した…？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_881", "\\E3しかし、&それより…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_882", "\\E0オレが&たべる&ぶんは、&のこって&るのか…？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_883", "\\TS \\F0 \\E0 \\T0 %")
	ds_map_add(global.text_data_ja, "SCR_TEXT_884", "＊ (スパゲティについて&　 なんと　こたえる？)& \\>0のこってない\\>1のこした\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_885", "   ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_890", "ホント&にッ？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_893", "\\E2うわあ…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_894", "オレさまが&つくった&りょうり、&ぜんぶ&たべて&くれた&ひと、&はじめて&だよ…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_895", "\\E0よぅしッ！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_900", "\\E2うわあ…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_901", "オレさまの&おてせい&パスタ、&のこした&の…？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_902", "オレさまの&ぶんを、&わざわざ&のこして&おいて&くれた&の…？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_905", "\\E0しんぱい&には、&およばん^1！&マスター&シェフ、&パピルス&さまが…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_906", "また&いくらでも&パスタを&ゆでて&くれる&わッ！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_907", "ハハハハ&ハハャニ！/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_911", "きた！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_912", "さっきの&ニンゲン&だッ！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_913", "\\E0きさまは、&ぜったいに&この&パズルを&きにいる&はずだ！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_914", "なにしろ、&かの&ゆうめいな&アルフィー&はかせが、&はつめい&したんだ&からな！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_915", "この&はいいろの&タイル&は…！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_916", "スイッチを&いれると…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_917", "いろが&かわる！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_918", "タイルは、&いろに&よって、&ちがった&こうかを&はっきする&のだッ！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_919", "あかい&タイルは、&「つうこう&きんし」！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_920", "つまり、&とおれ&ません！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_921", "きいろい&タイルは、&でんげき&ショック！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_922", "ふむと&かんでん&するぞ！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_923", "みどりの&タイルは、&けいほう&そうち！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_924", "うっかり&ふむと…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_925", "モンスター&と&せんとうに&なるッ！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_926", "オレンジの&タイルは、&オレンジの&かおり！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_927", "ふむと^1、&からだが、&おいしそう&な、&ニオイに&なるぞ！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_928", "あおい&タイルは、&みずの&タイル！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_929", "およいで&とおっても&かまわん^1！&ただし…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_930", "からだが&オレンジの&においに&なっている&と…！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_931", "ピラニアが&よってきて&かみつく&から、&ちゅうい&だッ！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_932", "それと^1、&あおい&タイルが…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_933", "きいろい&タイルの&となりだ^1と&あおい&タイルを&ふんだ&ときも、&かんでん&するぞ！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_934", "むらさきの&タイルは、&ツルツル&すべる！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_935", "ふむと、&となりの&タイルに&きょうせい&てきに、&いどうして&しまうッ！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_936", "ただし^1！&ツルツルの&もとになる&せっけん&は…/")
