	ds_map_add(global.text_data_ja, "SCR_TEXT_5561", "＊ (ねむれるイヌ^1は&　 おこして　たたかうべからず^1)&＊ (…という　ことわざがある)/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5566", "＊ (コンコン)/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5569", "＊ うわああああ^1！&＊ ルームサービスうぅぅ！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5570", "＊ 「ビチャビちゃ」　もってきてくれた？& & \\>0はい\\>1いいえ\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5571", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5575", "＊ ルームサービス…　けっきょく&　 こなかったな^1…&＊ (はぁ…)/%%")
