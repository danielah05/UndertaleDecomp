	ds_map_add(global.text_data_ja, "SCR_TEXT_2336", "\\E2そろそろ&デートを&はじめる？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2337", "\\TS \\F0 \\T0 %")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2338", "＊ (デートを　はじめる？)& & \\>0はい\\>1いいえ\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2339", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2348", "\\E0よしッ^1！&じゃあッ！&デート&はじめッ！/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2349", "\\E0よしッ^1！&じゃあッ！&ガッツリ&きあい&いれて&まったり&するぞッ！/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2354", "\\E2そっか^1…&じゃあ、&どうぞ&ごゆっくり/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2359", "＊ (この　ゆうびんうけには&　 「パピルス」と　かいてある)/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2360", "＊ (なかをみる？)& & \\>0はい\\>1いいえ\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2361", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2366", "＊ (からっぽだ)/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2368", "＊ (ほうりつに　いはんするかも&　 しれないので　やめた)/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2372", "＊ どうかしたか^1？&＊ オイラ　しごとを&　 かけもち　してんのさ。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2373", "\\E1＊ おかげで&　 きゅうけいじかんも&　 2ばい。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2374", "\\E0＊ いまから　グリルビーズ&　 いくけど　くるかい？& \\>0うん\\>1いそがしい\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2375", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2376", "＊ …/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2380", "\\E2＊ アンタ^1　こころを&　 いれかえた　みたいだな。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2381", "\\E0＊ グリルビーズで&　 ひるめしでも　おごるぜ？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2382", "\\E1＊ アンタが　こわがらせて&　 いなくなっちまったヤツらが&　 もどってきたらな…/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2389", "＊ そこまで　いわれちゃ&　 しょうがない。しごとを&　 きりあげて　いくか…/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2394", "＊ そっか^1。んじゃな。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2399", "\\E1＊ おっと^1…　せきを&　 えらぶときは&　 きをつけな。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2400", "＊ どっかのアホが&　 ブーブークッション&　 しかけてたりするからな。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2401", "\\E0＊ んじゃ　とりあえ^1ず&　 オーダーしようぜ。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2402", "＊ なに　くう…？& & \\>0ポテト\\>1バーガー\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2403", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2407", "＊ よし^1　ちょっとまってな。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2411", "＊ あー^1　いいねえ。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2412", "＊ よう　グリルビ^1ー&　 ポテトを　2つ&　 たのむぜ。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2418", "＊ ああー^1　いいねえ。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2419", "＊ よう　グリルビ^1ー&　 バーガー　2つ&　 たのむぜ。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_242", "＊ キッシュを　じめんにお^1き&　 すぐもどると　つたえた。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2426", "＊ ところでさ^1 &　 アンタ　どうおもう…？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2427", "＊ パピルスのこと。& & \\>0イケてる\\>1ダサい\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2428", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2434", "＊ ま　とーぜんだな。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2435", "\\E1＊ アンタも　あの&　 コスチュームを　きれば&　 イケてるスターになれる。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2436", "\\E0＊ アイツ　よほどのことが&　 ないかぎり^1　あれを&　 ぬがないからな。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2437", "＊ あ　でも　あんしんしな^1。&　 ちゃんと　まいにち&　 あらってるから。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2438", "\\E1＊ きたまま&　 シャワーあびてるって&　 イミだけど。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2442", "＊ …わかってないな。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2443", "\\E1＊ オイラに　ひにくは&　 つうようしないぜ？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2444", "\\E0＊ パピルスは&　 ホンモノのスターだ。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2445", "＊ オイラに　けいびいんの&　 しごとを　すすめたのも&　 アイツだしな。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2446", "＊ アンタには　わからない&　 だろうけど…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2447", "＊ なまけてるときに&　 しかってくれるヤツが&　 いるのは　いいもんだぜ…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2448", "\\E1＊ ま　オイラが　なまける&　 なんて　ありえない&　 はなしだけどな。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2453", "＊ メシが　きたぜ。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2454", "＊ ケチャップ　かけるか^1？& & \\>0はい\\>1いいえ\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2455", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_246", "＊ いじげんボックスを　つかう？& & \\>0はい\\>1いいえ\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2461", "\\E2＊ “ボーン”ナペティ。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2465", "\\E2＊ じゃ　オイラが　ぜんぶ&　 もらうぜ。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_247", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2472", "\\W＊ \\Yことばをはなす　はな\\Wって&　 きいたこと　あるかい？& \\>0はい\\>1いいえ\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2473", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2477", "\\E1＊ そっか。&　 もう　しってたか。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2478", "\\E1＊ じゃあ&　 おしえてやるよ…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2479", "\\W＊ そいつは^1…&　 \\E0\\Bエコーフラワー\\Wっ&　 つって…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_248", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2480", "＊ ぬまちの　あちこちに&　 はえてる。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2481", "＊ このはなに　こえかける^1と&　 そのことばを　なんども&　 くりかえすんだ…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2482", "＊ だからなんだ　って？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2483", "＊ パピルスが^1さ&　 このまえ　ヘンなこと&　 いってたんだ。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2484", "＊ 「ひとりでいると^1 &　 ときどき…」/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2485", "＊ 「はなが　1りん&　 あらわれて　はなしかけて&　 くる」　って。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2486", "＊ ほめてくれたり^1…&＊ アドバイスくれたり^1…&＊ はげましてくれたり…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2487", "＊ よげんを　したりするって&　 いうんだけど…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2488", "＊ …おかしいよな？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2489", "＊ きっと　だれかが&　 エコーフラワーつかって&　 からかってんだ。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2490", "＊ アンタも&　 きをつけといて　くれよ。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2491", "＊ たのんだぜ。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2495", "\\E1＊ そうそう…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2496", "\\E0＊ オイラ　きんけつだか^1ら&　 おかんじょうも&　 たのんでいいか？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2497", "＊ 10000G　ポッキリだ。& & \\>0はい\\>1いいえ\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2498", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2502", " %%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2506", "＊ くうそうの　つばさが　どんどん&　 ひろがる…　こんなきぶんは&　 ほんとうに　ひさしぶりです…/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2507", "＊ もう　かえってください。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2510", "＊ あなたは^1…&＊ そとのせかいから　きたのですね^1？&　 そうなのですね！？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2511", "＊ そとから　きたひとに&　 であえるなんて&　 めったにないことだ…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2512", "＊ どうか^1　おねがいです！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2513", "＊ そとのせかいのこと　おしえて！& &\\>0はい\\>1いいえ\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2514", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2518", "＊ さあ^1　はやく　みてきてください！/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2522", "＊ ああ^1！&＊ もどってきましたね！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2523", "＊ なかは　ここと　ちがいましたか？& &\\>0ちがう\\>1おなじ\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2524", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2535", "＊ え^1？&＊ 「ちじょう」^1？&＊ なんですか　それは？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2536", "＊ わたしが　ききたいのは&　 このへやの　そとのことです。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2537", "＊ ごらんのとおり^1　わたしの&　 “じく”は　じめんに　くっついて&　 はなれないので…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2538", "＊ だから^1！　どうか　おねがいです！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2539", "＊ ちょっと　かくにんしてきて&　 もらえませんか？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2540", "＊ わたしは　ずっと　このへやの&　 このばしょから　うごけずに&　 いきてきました。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2541", "＊ でも　ずっと　きになって&　 いたんです^1。みぎのへやには&　 なにがあるのか…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2542", "＊ ずっと　ゆめみてきたんです^1。&　 あのへやに　はいって　けしきを&　 かえることを…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2543", "＊ いや^1…　けしきだけ　じゃない…&＊ わがじんせいを　かえることを…！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2544", "＊ おねがいです^1！　なかを　みてきて&　 ください。そして　どんなだったか&　 おしえてください。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2553", "＊ そとの　ひとたちは　みんな&　 あなたのように　つめたいの&　 ですか…^1？　ひどいな…/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2561", "＊ そうか^1！　それは　よかった^1！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2562", "＊ それなら　わたしは　これからも&　 ゆめを　みつづけられます。&＊ あなたの　おかげですよ。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2567", "＊ おなじ…なんですか…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2568", "＊ そうですか…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2569", "＊ おなじか…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2570", "＊ …/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2571", "＊ わかりました。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2580", "＊ (かさを　かりる？)& & \\>0かりる\\>1かりない\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2581", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2585", "＊ (かさを　かえす？)& & \\>0かえす\\>1かえさない\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2586", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2597", "＊ (かさを　かりた)/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2608", " %%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2615", "＊ (かさを　かえした)/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2626", " %%")
