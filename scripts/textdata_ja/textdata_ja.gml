	ds_map_add(global.text_data_ja, "SCR_TEXT_5309", "\\E0＊ うふふふふ^1。&＊ やっぱり。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5310", "\\E1＊ このせかいに　ニンゲンが&　 おちてくるとね…/")
