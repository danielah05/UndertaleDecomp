	ds_map_add(global.text_data_ja, "SCR_TEXT_3488", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_3495", "そのハートはね^1  &キミのタマシイさ^1 &キミという　そんざい&そのもの　といってもいい/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_3496", "はじめは　すごくよわい^1…&けど^1　LVがたくさん&あがると^1 どんどん&つよくなれるんだよ/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_3497", "LVっていうのは^1 &LOVE^1　つまり^1  &「あい」のことさ！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_3498", "キミも　LOVEが&ほしいでしょ？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_3499", "まってね^1…&いま　ボクが　LOVEを&わけてあげるから！/%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_3503", "このせかいではね^1 &LOVEは　こんなふうに^1…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_3504", "しろくて　ちっちゃな^2…\\E1 &「なかよしカプセル」に&いれて　プレゼントするんだ/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_3505", "\\E2それじゃあ　いくよ\\E0？/%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_3509", "さあ！　カプセルを&おいかけて^1！&いっぱい　いーっぱい&ひろってね^2！%%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_3510", "%%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_3514", "バカだね/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_3515", "このせかいでは^1 &ころすか&ころされるかだ/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_3516", "こんな&ぜっこうの　チャンスを&のがすわけ　ないだろ！/%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_352", "\\E1＊ どちらが　ひだりかは&　 わかるわよね？/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_3520", "しね/%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_3524", "あれあれ^1？&ぜんぶ　おとしちゃったよ？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_3525", "もういっかい^1　いくね？/%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_3528", "ふざけてんの^2？&バカなの^2？&さっさと　あたれよ…&たｍ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_3531", "オマエ^1…&さては　しらないフリ&してるだけだな？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_3532", "ボクのこと&バカにしてんだろ/%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_3535", "\\E1なさけないわね^1… &つみもないこども^1を &いじめるなんて…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_3536", "\\E2こわがらなくても&だいじょうぶよ/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_3537", "\\Xわたしは　\\Bトリエル\\X&この\\Rいせき\\Xの&かんりにん　です/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_3538", "まいにち　ここを&みまわって^1 &おちてきたコが　いないか&かくにんしているの/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_3539", "ニンゲンが&このせかいに　きたのは&ほんとうに　ひさしぶり/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_3540", "あなたのことは&わたしが　ちゃんと&まもってあげますよ/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_3541", "\\E2さ　いきましょう^2！&いせきを　あんない&してあげるわ/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_3542", "%%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_3548", "＊ わたしぶね。3Gで　どう？& & \\>0のる\\>1のらない\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_3549", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_3553", "＊ それじゃ^1　また。/%%")
