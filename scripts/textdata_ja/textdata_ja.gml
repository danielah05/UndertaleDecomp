	ds_map_add(global.text_data_ja, "SCR_TEXT_938", "ピラニアは&この&レモンの&かおりが&だいッ&キライッ！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_939", "つまり、&むらさきと&あおの&くみあわせ&は、&セーフ&という&ことだッ！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_940", "そして&さいごに^1、&ピンクの&タイル！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_941", "ピンクは、&ふんでも&なにも&おこらない/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_942", "だから、&すきなだけ&ふむといい/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_943", "どうだ^1！&わかった&か！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_944", "\\TS \\F0 \\E0 \\T0 %")
	ds_map_add(global.text_data_ja, "SCR_TEXT_945", "＊ (せつめいを　りかいできた？)& & \\>0まかせて\\>1わからん\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_946", "   ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_953", "よし^1！&では、&さいごに&だいじな&ことを、&おしえよう/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_954", "この&パズルは…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_955", "かんぜん^1に&ランダムに&せいせい&されるッ！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_956", "オレさまが&この&スイッチを&おせば…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_957", "だれも&みたことの&ない&パズルが&たんじょう&するのだ！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_958", "だから、&こたえは、&オレさま&にも、&わからん！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_959", "ニャッハッ&ハッ^1！&かくごは&いいか…！/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_965", "そうか^1…&では、&もういちど&せつめい&しよう…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_966", "\\E0あかい&タイルは、&つうこう&きんし！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_967", "きいろい&タイルは、&かんでん&するから&キケン！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_968", "あおい&タイルは、&ふむと、&モンスター&と&せんとうに&なって…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_969", "みどりの&タイルは、&みずの&タイルで…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_970", "オレンジの&タイルは、&オレンジの&かおりつき/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_971", "オレンジを&ふんだら^1、&みどりは&ふむな！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_972", "\\E3ちゃいろの&タイルは…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_973", "\\E1まて^1！&ちゃいろの&タイルは&なかった…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_974", "\\E0むらさきの&タイルは、&レモンの&かおりで…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_975", "\\E3そういえ^1ば&どうして、&きいろが&レモンの&かおりじゃ&ないのかな/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_976", "\\E3うーん…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_977", "\\E1まって^1！&みどりと&あおの&せつめいが&ぎゃく&だった&かも！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_978", "えっとね、&あおの&ほうが、&みず&だからね！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_979", "\\E3で、&ピンクの&タイルは…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_980", "えっと^1…&ピンクは、&なん&だっけ…？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_981", "\\E1そうだ！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_982", "\\E3ピンクは、&ふんでも&なにも&おこらない/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_983", "\\E0よし^1！&どうだッ！&これで&すこしは&りかい&できた&かッ！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_984", "\\TS \\F0 \\E0 \\T0 %")
	ds_map_add(global.text_data_ja, "SCR_TEXT_985", "＊ (せつめいを　りかいできた？)& \\>1よけい& \\>0わかった\\>1わからん\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_986", "   ")
