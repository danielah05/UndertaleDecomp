	ds_map_add(global.text_data_ja, "SCR_TEXT_2652", "＊ (せきぞうの　なかから^1 &　 オルゴールのおとが&　 きこえはじめた…)/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2667", " %%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2674", "＊ (サビだらけの&　 ふるい　れいぞうこだ)/")
