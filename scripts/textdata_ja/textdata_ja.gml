	ds_map_add(global.text_data_ja, "SCR_TEXT_1989", "もしか&して…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_199", "＊ (あたりに　オゾンのにおいが&　 じゅうまんしている^1…&　 ケツイが　みなぎった)/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1990", "\\E3じぶんには&オレさまと&ともだちに&なる&しかくが&ないとか、&おもって&る？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1991", "\\E0そんなこと&ないよッ^1！&きさまは&サイコー&だッ^1！&オレさまが、&よろこんで&ともだちに&なって&あげるッ！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1996", "ウヒョウ^1！&デートに&いくまえ&から&おともだち&どまり&だなんて…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1997", "さすが&オレさま！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_1998", "ともだちを&つくる&には…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2002", "ウヒョウ！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2003", "おともだち&できちゃっ&たッ！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2004", "ともだちが&ほしかった&ら…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2006", "ダメダメな&パズルを&やらせて、&バトルを&すれば&よかった&んだねッ！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2007", "ニンゲン^1、&きさまは、&いろんな&ことを&おしえて&くれた/")
