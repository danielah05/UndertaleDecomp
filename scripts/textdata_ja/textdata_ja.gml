	ds_map_add(global.text_data_ja, "SCR_TEXT_2078", "＊ あら^1　あなたは　このまえの^1！&＊ しんぱいしてたのよ！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2079", "＊ でも　もうだいじょうぶよ^1。&　 あんしんして。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_208", "＊ (クモのすの　においが&　 あたりに　じゅうまんしている…)/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2080", "＊ わたしたち　モンスターは&　 みんな　ちじょうのせかいに&　 いじゅう　することに　なったの。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2081", "＊ ちじょうには　きっ^1と&　 あなたが　とまれる　ばしょも&　 あるはずよ！/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2089", "エラー。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_209", "＊ (ケツイが　みなぎった)/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2092", "＊ では　おへやのカギをどうぞ^1。&＊ あったかくして　おやすみなさい！/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2098", "＊ たったの　80Gも　はらえないの…？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2099", "＊ まあ　かわいそうに^1！&＊ きっと　これまで　たいへんな&　 くろうを　してきたのね…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2100", "＊ うえに　ひとつ　あきべやが&　 あるわ。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2101", "＊ そこに　とまっていきなさい。&＊ おだいは　いらないから。/%%")
