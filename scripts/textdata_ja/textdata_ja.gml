	ds_map_add(global.text_data_ja, "SCR_TEXT_707", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_7079", "＊ マネキンが　あらわれた。")
	ds_map_add(global.text_data_ja, "SCR_TEXT_7080", "%%%")
