	ds_map_add(global.text_data_ja, "obj_endflowey_372", "\\[C]…&どうして…/")
	ds_map_add(global.text_data_ja, "obj_endflowey_373", "\\E3いったい…/")
	ds_map_add(global.text_data_ja, "obj_endflowey_374", "\\E1どういうつもりだよ！/")
	ds_map_add(global.text_data_ja, "obj_endflowey_375", "\\E8ほんとうの　しょうりは&もくぜん　だったのに…/")
	ds_map_add(global.text_data_ja, "obj_endflowey_376", "\\E9あとすこしで&なにもかも&うまくいったのに！/")
	ds_map_add(global.text_data_ja, "obj_endflowey_377", "\\E2どうして&だいなしに　したの！/")
	ds_map_add(global.text_data_ja, "obj_endflowey_378", "\\E5…ハ…　ハハハ…/")
	ds_map_add(global.text_data_ja, "obj_endflowey_379", "\\E9ひょっとして&ふくしゅうの　つもり？/")
	ds_map_add(global.text_data_ja, "obj_endflowey_382", "\\E1ぜんにんの　ふりを&ボクに　みせつけるのが&もくてきだったの…？/")
	ds_map_add(global.text_data_ja, "obj_endflowey_386", "\\E1じゆうに　いきるさまを&ボクに　みせつけるのが&もくてき　だったの…？/")
	ds_map_add(global.text_data_ja, "obj_endflowey_389", "\\E1…/")
	ds_map_add(global.text_data_ja, "obj_endflowey_390", "\\E7いや…　ちがうね。/")
	ds_map_add(global.text_data_ja, "obj_endflowey_391", "\\E8そんなんじゃない。/")
	ds_map_add(global.text_data_ja, "obj_endflowey_392", "キミの　やってる&ことなんて&おみとおしだよ。/")
	ds_map_add(global.text_data_ja, "obj_endflowey_393", "キミは　ただ&どうなるか&みてみたかった　だけだ。/")
	ds_map_add(global.text_data_ja, "obj_endflowey_394", "そして　さいごには&かれらから　すべてを&うばうんだろ？/")
	ds_map_add(global.text_data_ja, "obj_endflowey_395", "あはははは…/")
	ds_map_add(global.text_data_ja, "obj_endflowey_396", "\\E9\\[C]^1 &キミは　てんさいだな。/")
	ds_map_add(global.text_data_ja, "obj_endflowey_397", "\\E4それな^1ら&きがすむまで&ためせばいいさ。/")
	ds_map_add(global.text_data_ja, "obj_endflowey_398", "\\E8どうせ　さいごは&ボクのところに&もどってくるんだ。/")
	ds_map_add(global.text_data_ja, "obj_endflowey_399", "\\E9それじゃ…/")
	ds_map_add(global.text_data_ja, "obj_endflowey_400", "\\E4\\[C]…/")
	ds_map_add(global.text_data_ja, "obj_endflowey_401", "\\E4ボクは　ここで&まっているよ。/%%")
	ds_map_add(global.text_data_ja, "obj_endflowey_415", "また　きたの？&なんで？/")
	ds_map_add(global.text_data_ja, "obj_endflowey_416", "\\E5ああ^1　そういうことか^1。&キミは　ボクとともだちに&なりたいんだね？/")
	ds_map_add(global.text_data_ja, "obj_endflowey_417", "\\E8くだらないな。/")
	ds_map_add(global.text_data_ja, "obj_endflowey_418", "\\E8ボクが　キミなんかと&ともだちに　なるわけ&ないじゃない。/")
	ds_map_add(global.text_data_ja, "obj_endflowey_419", "\\E5ハッ…/")
	ds_map_add(global.text_data_ja, "obj_endflowey_420", "\\E9ホントに　なにも&わかってないんだね。/")
	ds_map_add(global.text_data_ja, "obj_endflowey_421", "\\E5ボクが　きょうみを&もてた　あいては&1人しか　いない。/")
	ds_map_add(global.text_data_ja, "obj_endflowey_422", "\\E6それも　ほんとうに&そいつに　かんしんが&あったわけじゃない。/")
	ds_map_add(global.text_data_ja, "obj_endflowey_423", "\\E5ボクは　ただ…&「どこかにいる」と&おもいたいんだ…/")
	ds_map_add(global.text_data_ja, "obj_endflowey_424", "ボクを　あきさせない&だれかがね。/")
	ds_map_add(global.text_data_ja, "obj_endflowey_425", "\\E8ボクにそっくりな&だれかが　いるはずだと&おもいたいのさ。/")
	ds_map_add(global.text_data_ja, "obj_endflowey_426", "\\E8…/")
	ds_map_add(global.text_data_ja, "obj_endflowey_427", "\\E0ねえ。/")
	ds_map_add(global.text_data_ja, "obj_endflowey_428", "\\E4そんなマヌケな&かおで　ボクをみるの&やめてくれる？/")
	ds_map_add(global.text_data_ja, "obj_endflowey_429", "ムカつくから。/%%")
	ds_map_add(global.text_data_ja, "obj_endflowey_443", "\\E0またきたね。/")
	ds_map_add(global.text_data_ja, "obj_endflowey_444", "\\E2どういうつもり？/")
