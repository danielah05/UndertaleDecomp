	ds_map_add(global.text_data_ja, "SCR_TEXT_5372", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5379", "\\E1＊ へえ。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5380", "\\E0＊ そうか。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5381", "\\E3＊ …/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5390", "\\E1＊ そうか。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5391", "\\E0＊ ま^1　かんがえは&　 ひとそれぞれ　だから…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5392", "\\E2＊ オイラは　なにも&　 いうつもりは　ないぜ。&　 ただ…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5393", "\\E3＊ …/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_540", "＊ ボクのかけら^1　だいじにしてくれて&　 ありがとうございます…/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5402", "＊ プルルルル…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5404", "\\E0＊ もしもし！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5405", "\\E3＊ あのね^1　ぜんぜん&　 かんけいない&　 はなしなんだけど…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5406", "\\E2＊ …/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5407", "\\E6＊ こんど　いっしょに^1…&　 ニンゲンのアニメ　みない？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5408", "＊ ヒマなときに…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5409", "\\E4＊ えとね^1　&　 「キスキスキューティ&　 みゅうみゅう」っていうの。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5410", "\\TS \\F0 \\T0 %")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5411", " & & \\>0いいよ！\\>1…いやです\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5412", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5420", "＊ ほほほんとにっ！？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5421", "\\E3＊ すっごく　おもしろいの^1！&＊ いっちばん　すきな&　 アニメなのーっ！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5422", "＊ みゅうみゅうっていう&　 ニンゲンのおんなのこが&　 でてきてね！%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5423", "\\E3＊ ニンゲンなのに&　 ネコミミだから　すごく&　 きにしてるんだけどね！%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5424", "\\E6＊ でも　さいごには！%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5425", "＊ ネコミミなんて&　 かんけいない　って&　 きづくんだ！%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5426", "＊ ネコミミでも　みんな&　 みゅうみゅうのことが&　 だいすき…！%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5427", "\\E7＊ メッチャかんどう&　 だよ～！%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5428", "\\E5＊ あ　ゴメン&　 ネタバレしちゃった。%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5429", "\\E6＊ それと　みゅうみゅうには&　 とくしゅのうりょくが&　 あって！！%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5430", "\\E5＊ キスした　あいてを&　 マインドコントロール&　 しちゃうんだ！%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5431", "\\E3＊ こまったときは&　 キスして　あいてを&　 あやつって　かいけつ！%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5432", "\\E5＊ キスされたあいては&　 そのあとのことを&　 おぼえてないの！%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5433", "\\E3＊ でも　キスしようとして&　 しっぱいするとね！&＊ なんと！！%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5434", "\\E4＊ なんと^1…　えっと^1…&　 あ^1　そうそ^1う&　 もちろん…%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5435", "\\E5＊ さいごは^1　みゅうみゅうも&　 きづくんだ…　マインド&　 コントロールなんて…%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5436", "\\E3＊ あー　ごめん！&　 ぜんぶ　ネタバレ&　 しちゃダメだよね。%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5437", "\\E5＊ とにかく^1　あなたも&　 きっと　ハマると&　 おもうっ！ /")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5438", "\\E0＊ だから^1　このたびが&　 おわったら^1 &　 いっしょに　みようねっ！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5439", "\\TS \\F0 \\T0 %")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5440", "＊ ツー…/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5445", "\\E5＊ そ…　そか^1！　うん！&＊ べつに　いいよ！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5446", "＊ ちょっと^1　きいてみた…&　 だけだから！！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5447", "\\E6＊ でででも　きっと&　 いちど　みたら&　 ハマると　おもうなっ！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5448", "＊ だから　よかったら&　 みてみてねっ！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5449", "\\TS \\F0 \\T0 %")
	ds_map_add(global.text_data_ja, "SCR_TEXT_545", "＊ えっ…^1？　また…？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5450", "＊ ツー…/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5455", "\\M1＊ あ^1！&＊ おかえりなさいだお！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5456", "\\M0＊ すごいヒミツを　おしえてあげるお！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5457", "\\M1＊ じつは　オニオンさん…&　 バンドやってみることに&　 したんだお。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5458", "\\M1＊ バンドめいは&　 「レッドホット・ちびペッパーズ」&　 だお。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5459", "\\M0＊ でも　まだ　なまえしか&　 きめてないんだお。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_546", "＊ ごめんなさい^1…　これいじょう&　 あげたら^1　ボクのからだが&　 なくなっちゃいます…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5460", "\\M3＊ オニオンさんは^1 &　 がっき　できないお…&＊ うたも　ヘタだお。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5461", "\\M1＊ どうおもう！^1？&＊ にんきでると　おもう！？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5462", " & & \\>0おもう\\>1おもわん\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5463", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_547", "＊ やっぱり^1…　じりきで　いけない&　 ばしょへ　いきたいなんて&　 のぞんじゃ　いけなかったんだ。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5471", "\\M0＊ オニオンさんも^1　そうおもうお…/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5477", " %%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_548", "＊ ボクたちモンスターは　みんな&　 おなじ　うんめい…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5485", "＊ (ランプだ)/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5486", "＊ (あかりを　つける？)& & \\>0はい\\>1いいえ\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5487", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_549", "＊ ちていのせかいから^1　えいえんに&　 でることはできないんだ…/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5493", "＊ (でんきゅうが　ついていない^1)&＊ (ソケットには&　 かいちゅうでんとうが　さしてある)/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5494", "＊ (かいちゅうでんとうを　つける？)& & \\>0はい\\>1いいえ\\C")
