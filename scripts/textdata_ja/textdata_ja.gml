	ds_map_add(global.text_data_ja, "SCR_TEXT_2009", "それと、&ちじょうに&でる&ほうほう&だって、&おしえ&ちゃうぞ/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2010", "このまま&すすんで&いくと、&どうくつの&おわりに&つく/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2011", "\\Wそこが、&みやこだ^1…&そこにある&\\Yバリア\\Wを&ぬければ、&そとの&せかいに&でられる/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2012", "バリアは、&われわれを&ちていの&せかいに&とじこめて&いる、&まほうの&ふういん…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2013", "こちら&がわに&はいるのは&カンタン&だが^1、&そとへ&でると&なると、&はなしは&べつだ/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2014", "…つよい&タマシイを&もつもの&でないと、&バリアを&ぬける&ことは&できない…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2015", "…だが、&きさまなら&できる！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2016", "だから&王さまは、&ニンゲンを&つかまえ&ようと&している&のだ/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2017", "ニンゲンの&タマシイの&ちからで、&バリアを&こわす…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2018", "そうすれば&われわれ&モンスター&は、&ちじょうに&もどれる&のだ！/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2023", "えッ！&また&きたの？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2024", "そうか…&ほんとうの&りゆうが、&やっと&わかった&ぞ…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2025", "きさま…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2026", "オレさまの&かおを、&みたくて&みたくて、&しかたない&んだな…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2027", "そんな&ふうに&おもって&くれてる&ヤツと、&たたかう&わけには&いかないよ/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2028", "ていうか、&ぶっちゃ^1け&きさまを&つかまえる&のが、&めんど&くさく&なって&きた！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2029", "どう&する…^1？&まだやる？/")
