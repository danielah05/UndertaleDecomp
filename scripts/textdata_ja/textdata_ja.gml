	ds_map_add(global.text_data_ja, "scr_papcall_861", "ベンチに&いっぱい&ならんだ&キッシュに&まさる&ものなし！/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_866", "＊ ベンチに　いっぱい&　 ならんだ　キッシュに&　 まさるものは　ない！/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_87", "\\E3^1…&あんまり&やくに&たたない&のうりょく&だ！/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_876", "\\E3「ねがいの&ま」か^1…&きさまには&ねがいごと&って&ある…？/")
	ds_map_add(global.text_data_ja, "scr_papcall_877", "\\E0ニャハハ&ハハ^1！&オレさま&には、&あるぞ！/")
