	ds_map_add(global.text_data_ja, "SCR_TEXT_5317", "＊ ツー…/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5323", "\\E1＊ あら^1…　そうなのね。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5324", "\\E0＊ わかったわ^1。&＊ それじゃ　またあとでね。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5325", "\\TS \\F0 \\T0 %")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5326", "＊ ツー…/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_533", "＊ えっ^1…&＊ いま^1　ボクの　からだの&　 かけらを　たべました…？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5337", "＊ (いま　メタトンに　うしろを&　 むかせれば　ながいスピーチを&　 スキップできそうだ)/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5338", "＊ (どうする？)& & \\>0スキップ\\>1もういちどきく\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5339", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_534", "＊ ボクの　めのまえで！？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5345", "＊ (メタトンに&　 「うしろに　おもしろいものがある」&　 と　つたえた)/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_535", "＊ あなたとは　もう&　 くちも　ききたくありません^1…&＊ かえってください！/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5353", " %%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5359", "＊ ただ…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5360", "＊ ひとつだけ　どうしても&　 きになる　ことがあんだ。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5361", "＊ オイラも^1 &　 せいとうぼうえい　なら&　 しょうがないとおもうぜ。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5362", "＊ アンタは　いろいろと&　 めんどうに　まきこまれた&　 からな。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5363", "＊ ただ…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5364", "＊ アンタ　ときどき…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5365", "＊ なにが　おきるか&　 わかってるみたいに&　 こうどう　するだろ。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5366", "＊ ぜんぶ　いちど&　 けいけん　したこと&　 ある　みたいにさ。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5367", "＊ こんなこと　いうと&　 ヘンに　おもわれる&　 かもしれないけど…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5368", "\\W＊ もしアンタが　なにかの&　 \\Yとくしゅのうりょく\\Wを&　 もってんなら…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5369", "＊ ただしい　ことをする&　 せきにんが　あるんじゃ&　 ないか？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5370", "\\TS \\F0 \\T0 %")
