	ds_map_add(global.text_data_ja, "SCR_TEXT_5053", "\\E1＊ てことは^1　オイラも&　 ゴミに　まみれに&　 いけってことか。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5055", "\\E0＊ あら^1　じゃあ　わたしも&　 ゴミに　まみれに&　 いっていいかしら？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5057", "\\E1＊ えと^1…　どうぞ？/")
