	ds_map_add(global.text_data_ja, "SCR_TEXT_5313", "\\E1＊ むかしの　ともだちに&　 であったような&　 きもちに　なったのよ。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5314", "\\E0＊ ふしぎよね？/")
