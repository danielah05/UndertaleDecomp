	ds_map_add(global.text_data_ja, "SCR_TEXT_2752", "＊ (マネキンを　こぶしで&　 かるく　たたいた)/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2753", "＊ (もうしわけなくなった)/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2757", "＊ (マネキンを　かるく　なぐった)/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2758", "＊ (なにかを　しゅうとく&　 できたようには&　 おもえない)/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_276", "＊ アイテムを　もっていない^1。&＊ はこのなかを　ただみつめた。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2762", "＊ (マネキンを　ぶんなぐった)/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2763", "＊ (どうせ　ただのマネキンだ)/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2767", "＊ (マネキンに　ぜんりょくで&　 パンチを　おみまいした)/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2768", "＊ (いいきぶんだ)/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_277", "＊ アイテムを　もっていない^1。&＊ なにかを　いれるところを&　 そうぞうした。/%%")
