	ds_map_add(global.text_data_ja, "SCR_TEXT_5125", "\\E0＊ モンスターと　ニンゲンの&　 あいだを　とりもつ「大使」&　 に　なってくれないか？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5126", "\\TS \\F0 \\T0 %")
