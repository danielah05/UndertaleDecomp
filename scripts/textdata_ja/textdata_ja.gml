	ds_map_add(global.text_data_ja, "SCR_TEXT_3268", "\\E0でもッ^1！&しんぱい&いらない&からッ！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_3269", "パピルス&さまは、&きさまを&うらぎっ&たりは&しないッ！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_3270", "\\E3オレさまは&ワルモノ&じゃない！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_3271", "\\E0ようきな&いやし系&キャラを、&めざして&るんだッ！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_3272", "パピルス&さまは、&おつきさま&の&かおりの&ナイス&ガイッ！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_3273", "だから、&シンセツの&さいのうを&くしして…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_3274", "きさまは&\\[1]を&みにつけて&いないと、&ともだちに&つたえて&おいた！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_3275", "\\E3きさまは&たしかに、&「みに&つけてる」&って&いった&けどね！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_3276", "でも、&オレさまが&テキトーに&ごまかし&て…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_3277", "きさまは&いま…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_3278", "\\E0\\[2]を&みに&つけてると&つたえ&といたよ！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_3279", "\\E3あからさま&な&ウソをつく&のは、&こころが&いたんだ…/")
