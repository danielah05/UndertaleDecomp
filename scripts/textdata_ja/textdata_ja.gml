	ds_map_add(global.text_data_ja, "SCR_TEXT_7090", "%%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_7094", "＊ ナキムシが&　 うじうじと　ちかよってきた！")
	ds_map_add(global.text_data_ja, "SCR_TEXT_7095", "%%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_7099", "＊ フロギーと　ナキムシが&　 にじりよってきた！")
	ds_map_add(global.text_data_ja, "SCR_TEXT_7100", "%%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_7104", "＊ チビカビに&　 ゆくてを　ふさがれた！")
	ds_map_add(global.text_data_ja, "SCR_TEXT_7105", "%%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_711", " %")
	ds_map_add(global.text_data_ja, "SCR_TEXT_712", "＊ ナイスクリーム^1　いかがスかぁ！&＊ 舌にヒンヤリ^1　ココロはホッコ^1リ&　 おいしいフローズンスイーツだよ！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_713", "＊ いまだけ　たったの25G！& & \\>0かう\\>1かわない\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_714", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_721", "＊ おッ^1！　おきゃくさん&　 カサ　もってるね！&　 うちのカートと　おそろいだ。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_722", "＊ こりゃあ　とくべつに&　 わりびき　してあげないと&　 いけないね！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_723", "＊ アイスクリーム　15Gでいいよ！& & \\>0かう\\>1かわない\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_724", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_728", " %")
	ds_map_add(global.text_data_ja, "SCR_TEXT_729", "＊ カサが　おそろいッ…！^1？&＊ ってことは　わりびきしないと&　 いけないな…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_730", "＊ アイスクリーム　15Gでいいよ！& & \\>0かう\\>1かわない\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_731", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_736", "＊ おッ^1！&＊ パンチカードが　3まい&　 たまったね！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_737", "＊ ナイスクリームと&　 こうかんするかい？/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_738", "＊ タダだよ！& & \\>0はい\\>1いいえ\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_739", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_777", "＊ まいどありッ^1！　はい　どうぞ^1！&＊ はこから　カードをとってね！&＊ (ナイスクリームを　てにいれた)/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_779", "＊ あれッ^1？　おカネないの^1？&＊ わるいけど^1　タダでは&　 あげられないよ。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_781", "＊ え^1？　わりびきしたのに&　 それでも　おカネたりない^1？&＊ そっか…/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_784", "＊ にもつが　おおすぎる&　 みたいだね^1！&＊ ざんねん！/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_803", "＊ りょーかいッ^1！　はい　どうぞ^1！&＊ (カードを　3まい　わたして&　 ナイスクリームを1つ　てにいれた)/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_809", "＊ じゃあ^1…&＊ おともだちに　つたえておいてよ…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_810", "＊ みずのながれる　どうくつのおくに&　 アイスクリームショップが&　 あるよ　って。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_815", "＊ オイラも　おやつビジネス&　 はじめてみたよ。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_816", "＊ 「フライドスノー」^1。&＊ 5G　ポッキリだ。& \\>0ください\\>1いらない\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_817", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_823", "＊ オイラ　5Gっていった^1？&＊ 50Gの　まちがいだ^1。& \\>0ください\\>1いらない\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_824", "  ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_828", "＊ だよな。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_829", "＊ それじゃ　さすがに&　 やすすぎるよな。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_836", "＊ ホントに^1？&＊ じゃあ　5000Gだったら？& \\>0ください\\>1いらない\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_837", "  ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_841", "＊ だよな。/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_842", "＊ それでも　まだ&　 やすすぎだ。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_849", "＊ じゃあ　50000G^1。&＊ これで　きまりだ。& \\>0ください\\>1いらない\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_850", "  ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_854", "＊ わるいな…&　 どうしても　ざいりょうひ&　 が　かかるからさ。/%%")
