	ds_map_add(global.text_data_ja, "scr_papcall_544", "\\E0パピルス&って&だれの&ことか、&わかって&るッ？/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_554", "オレさまの&いえに&いるのか^1！&かしこい&はんだん&だッ！/")
	ds_map_add(global.text_data_ja, "scr_papcall_555", "\\E3…まあ、&せいかく&には、&兄ちゃんの&いえでも&あるけどね/")
	ds_map_add(global.text_data_ja, "scr_papcall_556", "\\E1でも^1、その&はなしは&あんまり&したく&ないッ！/")
	ds_map_add(global.text_data_ja, "scr_papcall_557", "\\E3兄ちゃんの&へやは^1、&べつの&せかいだと&おもえッ！/")
	ds_map_add(global.text_data_ja, "scr_papcall_558", "\\E2そうじきの&つかいかた&を&しらない&ヤツらの&せかいだ！/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_563", "兄ちゃんの&へやは、&フシギ&くうかん&だ…&ちかよら&ないに&こした&ことはない/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_572", "オレさまが&きさまを&つかまえ&ようとした&ばしょ&だなッ！/")
	ds_map_add(global.text_data_ja, "scr_papcall_573", "おもいだす&のも&ツラい&よッ！/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_578", "…/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_58", "かわで&およぐ&には、&さむ&すぎるよ！/")
	ds_map_add(global.text_data_ja, "scr_papcall_584", "＊ そこは^1 &　 スノーレスリングに&　 ぴったりの　ばしょだ！/")
	ds_map_add(global.text_data_ja, "scr_papcall_586", "スノー&レスリング&っていう&のは、&アンダイン&が&うしろから&ちかづいて&きて…/")
	ds_map_add(global.text_data_ja, "scr_papcall_587", "たいあたり&されて、&「スノー&レスリング&だーッ！」&って&いわれる&あそびだ！/")
	ds_map_add(global.text_data_ja, "scr_papcall_588", "\\E5しょうじ^1き&すっごい&めいわく！/")
	ds_map_add(global.text_data_ja, "scr_papcall_59", "ぼうすい&セーターを&きれば、&べつ&だけどッ！/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_590", "\\E9＊ …やれやれ^1 &　 そこまで　いわれちゃ&　 しかたないな。/")
	ds_map_add(global.text_data_ja, "scr_papcall_592", "…え/")
	ds_map_add(global.text_data_ja, "scr_papcall_594", "＊ スノーレスリングだーッ！/")
	ds_map_add(global.text_data_ja, "scr_papcall_596", "やめてえぇ&ぇぇぇ！/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_601", "＊ おまえも　こんど&　 いっしょに　やろうな！/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_610", "ウォーター&フェルか^1…&そこの&ことは、&ほとんど&しらないよ/")
	ds_map_add(global.text_data_ja, "scr_papcall_611", "\\E0でも、&がんばって&いろいろ&おしえる&からねッ！/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_616", "あー^1…&でも、&その&エリアの&ことは、&なにも&しらない&です…/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_622", "＊ スノーフルに　かんしては&　 とくに　コメントはない。/")
	ds_map_add(global.text_data_ja, "scr_papcall_624", "オレさまは&あるぞ！/")
	ds_map_add(global.text_data_ja, "scr_papcall_626", "＊ …いってみろ。/")
	ds_map_add(global.text_data_ja, "scr_papcall_628", "…まって^1！&いま、&かんがえて&るから！/")
	ds_map_add(global.text_data_ja, "scr_papcall_630", "＊ …つまり^1　「ない」って&　 ことだろ！？/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_635", "いま^1、&スノーフル&にかんする&コメントを&かんがえて&るよッ！/")
	ds_map_add(global.text_data_ja, "scr_papcall_637", "＊ かんがえてる　じてんで&　 「コメントは　ない」&　 ということだ。/")
	ds_map_add(global.text_data_ja, "scr_papcall_639", "ちがうよ！&あるよ！/")
	ds_map_add(global.text_data_ja, "scr_papcall_640", "\\E5まってね！&もう&ちょっと&かんがえ&れば、&きっと&おもい&つくから！/")
	ds_map_add(global.text_data_ja, "scr_papcall_642", "＊ やっぱり&　 ないんじゃないかッ！！/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_65", "ただの&かわだよ/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_660", "そこも、&兄ちゃんの&もちばだよ/")
	ds_map_add(global.text_data_ja, "scr_papcall_661", "\\E3そう^1、&兄ちゃん&は、&いちどに&二かしょ&も、&けいび&してる&んだッ！/")
	ds_map_add(global.text_data_ja, "scr_papcall_662", "\\E0すごい&だろう！/")
	ds_map_add(global.text_data_ja, "scr_papcall_663", "つまり、&ふつうの&二ばいも&サボッて&るって&ことだ！/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_668", "ぼんじん&には&マネ&できない&はなれわざ&だよ…/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_674", "＊ そこの　けいびは&　 パピルスの　あにきが&　 たんとうしてる。/")
	ds_map_add(global.text_data_ja, "scr_papcall_675", "\\E4＊ アイツは　いつも&　 いねむりしてるか&　 サボってるか　だ。/")
	ds_map_add(global.text_data_ja, "scr_papcall_676", "\\E2＊ それでも　ちゃっかり&　 うまいことやるから&　 クビには　できない。/")
	ds_map_add(global.text_data_ja, "scr_papcall_677", "\\E4＊ …まあ^1　きょうは&　 べつだがな。/")
	ds_map_add(global.text_data_ja, "scr_papcall_678", "\\E2＊ おまえのことを&　 わたしに　いっさい&　 ほうこくしなかったから。/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_683", "\\E4＊ いつも　ひつよう&　 さいしょうげん^1…&　 ギリギリのしごとをする。/")
	ds_map_add(global.text_data_ja, "scr_papcall_684", "\\E9＊ それって　あるいみ&　 すごいのかも…？/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_693", "いわが、&おちて&くる…？/")
	ds_map_add(global.text_data_ja, "scr_papcall_694", "\\E0それは&きっと、&アンダイン&が&つくった、&ハイレベル&なパズルだ/")
	ds_map_add(global.text_data_ja, "scr_papcall_695", "\\E0きをつけた&ほうが&いいぞッ！/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_7", "＊ プルルルル^1…&　 プルルルル…/")
	ds_map_add(global.text_data_ja, "scr_papcall_700", "いわに&ちゅうい&すれッ！/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_706", "＊ そう^1　ここには&　 わたしが　パズルを&　 つくるはずだった。/")
	ds_map_add(global.text_data_ja, "scr_papcall_707", "\\E2＊ だが　わたしは&　 パズルが　だいッキライだ。/")
	ds_map_add(global.text_data_ja, "scr_papcall_708", "\\E6＊ だから　じょうりゅうに&　 いわを　やまほど&　 つんで　おいたんだ。/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_71", "ニャハハ^1！&おどろいた&かッ！/")
	ds_map_add(global.text_data_ja, "scr_papcall_714", "＊ なんだ^1！&＊ もんくあるかッ！/")
	ds_map_add(global.text_data_ja, "scr_papcall_715", "\\E6＊ わたしが　てをぬいた&　 おかげで^1　おまえは&　 たすかったはずだぞ！/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_72", "オレさまは&パズルの&てんさいで&ある&ばかりか…/")
	ds_map_add(global.text_data_ja, "scr_papcall_722", "＊ プルルルル^1…&　 プルルルル…/")
	ds_map_add(global.text_data_ja, "scr_papcall_723", "＊ (だれも　でない…)/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_73", "いだいな&けんちくか&でも&あるの&だッ！/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_731", "ちょっと！&どこいった&のッ？&みえない&よッ！/")
	ds_map_add(global.text_data_ja, "scr_papcall_732", "\\E0あ^1…&そっか、&でんわ&だから&あたりまえ&か…/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_737", "もしもし^1？&きってない&よねッ？/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_743", "＊ たきの　うらの&　 かくしべやを　みつけたか…/")
	ds_map_add(global.text_data_ja, "scr_papcall_744", "\\E0＊ のんびり　きゅうけい&　 したいとき^1　わたしは&　 いつも　そこへいく。/")
	ds_map_add(global.text_data_ja, "scr_papcall_745", "\\E1＊ のんびり　なんて&　 まっぴらだか^1ら　ぜったい&　 いかない　ということだッ！/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_750", "＊ わたし^1は&　 いかりと　ストレスが&　 だいすきなんだあッ！/")
	ds_map_add(global.text_data_ja, "scr_papcall_751", "\\E0＊ …なぁんてな^1。&　 じょうだんだよ。/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_757", "＊ プルルルル^1…&　 プルルルル…/")
	ds_map_add(global.text_data_ja, "scr_papcall_758", "＊ (だれも　でない…)/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_766", "ゲッ^1…&さっき&ここで、&ちょっと&きまずい&じけんが&あった…/")
	ds_map_add(global.text_data_ja, "scr_papcall_767", "\\E0きさまに&みられて&なくて、&ホントに&よかった！/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_772", "きさまに&みられて&なくて&よかった！/")
	ds_map_add(global.text_data_ja, "scr_papcall_773", "うれしい&こと&だから^1、&二回&いっちゃっ&たッ！/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_779", "＊ そこは^1　わたしが&　 はじめて　おまえを&　 みつけた　ばしょだな。/")
	ds_map_add(global.text_data_ja, "scr_papcall_780", "\\E2＊ あのまま　ヤリを&　 ガシガシうちこんで&　 やりたかったが…！/")
	ds_map_add(global.text_data_ja, "scr_papcall_781", "\\E9＊ その海草は　かがくてきに&　 じゅうようだか^1ら&　 きずつけられなかった。/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_786", "＊ なんだッ^1！&＊ ほんとうの　はなしだぞッ！/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_79", "ダンボール&は、&兄ちゃんも&いっしょに&さがして&くれたん&だッ！/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_792", "＊ プルルルル^1…&　 プルルルル…/")
	ds_map_add(global.text_data_ja, "scr_papcall_793", "＊ (だれも　でない…)/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_8", "＊ (だれも　でない…)/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_801", "あしばの&タネは^1、&４つ&ならぶと&はながさく/")
	ds_map_add(global.text_data_ja, "scr_papcall_802", "\\E3でも、&それって&すごく&ふべんだ/")
	ds_map_add(global.text_data_ja, "scr_papcall_803", "\\E2どうせ&なら、&ひこうきの&タネに&すれば&いいのに！/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_808", "それか^1、&ジェット&パックに&へんけい&する&でんわの&タネ&とかッ！/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_814", "＊ え^1…　おまえ&　 りちぎに　そのパズルを&　 といたのか！？/")
	ds_map_add(global.text_data_ja, "scr_papcall_815", "\\E2＊ フツーに　ジャンプで&　 とびこえられないのか！？/")
	ds_map_add(global.text_data_ja, "scr_papcall_816", "\\E6＊ ケンケンパーでも&　 ダメか！？/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_821", "＊ ケンパー　ケンパー&　 ケンケンパーでもか！？/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_830", "うーむ^1…&その&パズルを&とくには…/")
	ds_map_add(global.text_data_ja, "scr_papcall_831", "\\E0そうか！&わかった&ぞッ！/")
	ds_map_add(global.text_data_ja, "scr_papcall_832", "あしばの&タネで、&あしばを&つくれば&いいッ！/%%")
