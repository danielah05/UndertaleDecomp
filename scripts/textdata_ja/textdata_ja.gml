	ds_map_add(global.text_data_ja, "SCR_TEXT_5598", "＊ (ビチャビちゃを　ドアのしたの&　 すきまから　そそぎいれた)/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5599", "＊ うあああああぁぁ^2！&＊ サーブのしかたも　カンペキ！^1！&＊ チップあげるうぅぅ！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5600", "＊ (99G　てにいれた)/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5604", "＊ ^1…&＊ もってないだろ。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5609", "＊ じゃあ　なにしにきたの…！？/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5614", "＊ (コンコン)/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5617", "＊ うわああああ ^1！&＊ ルームサービスうぅぅ！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5618", "＊ 「シナモンキー」&　 もってきてくれた？ & \\>0はい\\>1いいえ\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5619", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5623", "＊ ルームサービス…　けっきょく&　 こなかったな^1…&＊ (はぁ…)/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5628", "＊ とんくす。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_563", "＊ もちものが　いっぱい^1？&＊ そうですか^1…　ボクのことなんて&　 けっきょく　どうでもいいんですね。/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5632", "＊ (へんじがない)/%%")
