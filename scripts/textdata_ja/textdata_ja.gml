	ds_map_add(global.text_data_ja, "SCR_TEXT_348", "\\E0＊ しるしがついている&　 スイッチをおすのよ。/%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_3482", "＊ (ピアノだ^1)&＊ (ひいてみる？)& \\>0はい\\>1いいえ\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_3483", " ")
