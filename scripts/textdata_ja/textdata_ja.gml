	ds_map_add(global.text_data_ja, "SCR_TEXT_5047", "\\E2＊ ちょッ！　アルフィー^1！&　 また　ゴミまみれに&　 するぞ！！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5049", "\\E0オレさまも&ゴミまみれ&に　なって&いい？/")
