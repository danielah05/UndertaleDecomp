	ds_map_add(global.text_data_ja, "SCR_TEXT_5136", "\\E0やった^1！&フリスクは&ぜったい、&じょうずな&大使に&なるよッ！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5137", "そして^1、&この&いだいなる&パピルス&さまは…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5138", "じょうずな&マスコット&に&なって&やるッ！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5139", "さっそく&ごあいさつ&しに&いって&くるねッ^1！&だいいち&いんしょう&だいじッ！/%%")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5143", "\\E0しんぱい&ないよ、&フリスク^1！&オレさまに&まかせろ！/")
