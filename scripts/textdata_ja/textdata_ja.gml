	ds_map_add(global.text_data_ja, "SCR_TEXT_2740", "＊ (ボコッてみる？)& & \\>0はい\\>1いいえ\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2741", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2744", "＊ (これいじょう　マネキンに&　 ようはない)/%%")
