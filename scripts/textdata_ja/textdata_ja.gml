	ds_map_add(global.text_data_ja, "SCR_TEXT_2637", "＊ (せきぞうに　かさを&　 さしてあげる？)& \\>0はい\\>1いいえ\\C")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2638", " ")
	ds_map_add(global.text_data_ja, "SCR_TEXT_2642", "＊ (おんがくは　なりつづけている^1。&　 とまる　けはいは　ない)/%%")
