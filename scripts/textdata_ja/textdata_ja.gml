	ds_map_add(global.text_data_ja, "SCR_TEXT_3314", "\\E3ホントは…/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_3315", "\\E0「\\[1]を&みにつけ&てるよ！」&ってイミ&だったん&だよねッ？/")
