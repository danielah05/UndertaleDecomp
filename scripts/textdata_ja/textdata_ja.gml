	ds_map_add(global.text_data_ja, "scr_papcall_93", "ワンボーは&るすだよ/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_930", "\\E2＊ どうして　ニンゲンの&　 れきしのように&　 アニメじゃないんだ！/")
	ds_map_add(global.text_data_ja, "scr_papcall_931", "\\E6＊ せめて　つるぎと&　 キスシーンぐらいは&　 きざんで　ほしいぞッ！/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_936", "＊ ケンソンするな^1！　だれが&　 みたって　ニンゲンの&　 れきしは　イケてるぞ！/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_942", "＊ プルルルル^1…&　 プルルルル…/")
	ds_map_add(global.text_data_ja, "scr_papcall_943", "＊ (だれも　でない…)/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_950", "まったく&あんぜん&で、&いっさい&キケンの&ない&エリアだ…/")
	ds_map_add(global.text_data_ja, "scr_papcall_951", "\\E0オレさまの&おかげ&だなッ^1！&ニャハハ！/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_956", "ハ！！/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_962", "＊ わたしが　おまえに&　 ヤリを　ガンガンなげた&　 ばしょだな…/")
	ds_map_add(global.text_data_ja, "scr_papcall_963", "\\E6＊ あーたのしかったーッ^1！&＊ また　やろうなッ！/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_968", "＊ また“やろう”なッ^1！&＊ “ヤリ”　だけにッ！？/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_974", "＊ プルルルル^1…&　 プルルルル…/")
	ds_map_add(global.text_data_ja, "scr_papcall_975", "＊ (だれも　でない…)/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_982", "あしもとに&きを&つけてッ！/")
	ds_map_add(global.text_data_ja, "scr_papcall_983", "\\E3アンダイン&は、&ここの&海草を&なぜか&すーっごく&だいじに&してるから/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_988", "ちゃんと&きをつけて&るッ？/%%")
	ds_map_add(global.text_data_ja, "scr_papcall_994", "＊ 海草さえなかった^1ら&　 おまえの　ケツに&　 ヤリを…/")
	ds_map_add(global.text_data_ja, "scr_papcall_995", "\\E9＊ あー^1　おまえの&　 おケツに　おヤリを&　 つっこんでやったのに。/%%")
