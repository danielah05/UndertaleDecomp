	ds_map_add(global.text_data_ja, "SCR_TEXT_5042", "\\E6＊ アズゴア^1！　やめろ^1！&＊ わたしのハートまで&　 ズタボロになる！/")
	ds_map_add(global.text_data_ja, "SCR_TEXT_5044", "\\E9＊ そうだよ　アズゴア^1…&＊ アンダインのハートを&　 きずつけないで。/")
