	ds_map_add(global.text_data_en, "obj_grillbynpc_foodmonster_77", "* I'd love to try it sometime./%%")
	ds_map_add(global.text_data_en, "obj_grillbynpc_foodmonster_80", "* And I've heard they have&  things called bathrooms./%%")
