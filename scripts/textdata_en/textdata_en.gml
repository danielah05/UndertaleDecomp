	ds_map_add(global.text_data_en, "obj_papyrus7_338", "\\E3HEH???/%%")
	ds_map_add(global.text_data_en, "obj_papyrus7_341", "A VALUABLE LIFE&LESSON!!!/")
	ds_map_add(global.text_data_en, "obj_papyrus7_342", "NYEH HEH HEH!!!/%%")
	ds_map_add(global.text_data_en, "obj_papyrus8_184", "HUMAN./")
