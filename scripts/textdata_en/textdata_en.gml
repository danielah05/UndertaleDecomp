	ds_map_add(global.text_data_en, "obj_floweybattle1_278", "stop it")
	ds_map_add(global.text_data_en, "obj_floweytrigger_122", "\\E1* Hello..^1.&* Are you all right?/")
	ds_map_add(global.text_data_en, "obj_floweytrigger_123", "* You must be so lost&  and confused.../")
	ds_map_add(global.text_data_en, "obj_floweytrigger_124", "\\E2* Ah, do not be&  afraid^1, my child./")
	ds_map_add(global.text_data_en, "obj_floweytrigger_125", "\\W* I am \\BTORIEL\\W,&  caretaker of the&  \\RRUINS\\W./")
	ds_map_add(global.text_data_en, "obj_floweytrigger_126", "* I pass through here&  every day to see if&  anyone has fallen down./")
	ds_map_add(global.text_data_en, "obj_floweytrigger_127", "* You are the first&  human to come here&  in a long time./")
	ds_map_add(global.text_data_en, "obj_floweytrigger_128", "* I will do my best&  to protect you during&  your time here./")
	ds_map_add(global.text_data_en, "obj_floweytrigger_129", "* Come^1!&* I shall guide you&  through the catacombs./%%")
	ds_map_add(global.text_data_en, "obj_floweytrigger_316", "\\W* Howdy^2!&* I'm\\Y FLOWEY\\W.^2 &* \\YFLOWEY\\W the \\YFLOWER\\W!/")
	ds_map_add(global.text_data_en, "obj_floweytrigger_317", "* Hee hee hee.../")
	ds_map_add(global.text_data_en, "obj_floweytrigger_318", "* Why'd you make me&  introduce myself?/")
	ds_map_add(global.text_data_en, "obj_floweytrigger_319", "* It's rude to act&  like you don't&  know who I am./")
	ds_map_add(global.text_data_en, "obj_floweytrigger_320", "* Someone ought to teach&  you proper manners./")
	ds_map_add(global.text_data_en, "obj_floweytrigger_321", "* I guess little old me&  will have to do./")
	ds_map_add(global.text_data_en, "obj_floweytrigger_322", "* Ready^2?&* Here we go!/%%")
	ds_map_add(global.text_data_en, "obj_floweytrigger_327", "* Don't you have anything&  better to do?/%%")
	ds_map_add(global.text_data_en, "obj_floweytrigger_338", "* Error/%%")
	ds_map_add(global.text_data_en, "obj_floweytrigger_342", "* Really^1, \\[C]?/")
	ds_map_add(global.text_data_en, "obj_floweytrigger_343", "\\E5* Well^1, do what you&  will./")
	ds_map_add(global.text_data_en, "obj_floweytrigger_344", "\\E4* I'll be waiting for&  you!/%%")
	ds_map_add(global.text_data_en, "obj_floweytrigger_348", "\\E0* Hey^1.&* Remember./")
	ds_map_add(global.text_data_en, "obj_floweytrigger_349", "\\E2* DON'T kill anyone./")
	ds_map_add(global.text_data_en, "obj_floweytrigger_350", "\\E2* .../")
	ds_map_add(global.text_data_en, "obj_floweytrigger_351", "\\E1* I can't believe this&  is a REAL thing I&  have to remind you./")
	ds_map_add(global.text_data_en, "obj_floweytrigger_352", "\\E0* Hee hee hee.../")
	ds_map_add(global.text_data_en, "obj_floweytrigger_353", "* Good luck!/%%")
	ds_map_add(global.text_data_en, "obj_floweytrigger_357", "* Remember.../")
	ds_map_add(global.text_data_en, "obj_floweytrigger_358", "\\E1* THIS time^1, you've&  GOT to become friends&  with everyone./")
	ds_map_add(global.text_data_en, "obj_floweytrigger_359", "\\E2* OK?/")
	ds_map_add(global.text_data_en, "obj_floweytrigger_360", "\\E5* If you DON'T^1, you'll&  be miserable FOREVER./")
	ds_map_add(global.text_data_en, "obj_floweytrigger_361", "\\E1* And we wouldn't want&  THAT^1, would we?/")
	ds_map_add(global.text_data_en, "obj_floweytrigger_362", "\\E0* No..^1.&* We just want you&  to be happy^1, right?/")
	ds_map_add(global.text_data_en, "obj_floweytrigger_363", "* Good luck. /%%")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_158", "* Error/%%")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_162", "* Hee hee hee.../")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_163", "\\E2* I hope you like your&  choice./")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_164", "* After all^1, it's not&  as if you can go&  back and change fate./")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_165", "\\E0* In this world^1, it's kill&  or be killed./")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_166", "\\E2* That old hag thought&  she could break the&  rules./")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_167", "\\E1* She tried so hard&  to save you humans./")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_168", "\\E0* But when it came&  down to it.../")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_169", "\\TF*\\E5 Hee hee.../")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_170", "\\E4* SHE COULDN'T EVEN&  SAVE HERSELF./")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_171", "* WHAT AN IDIOT!!/%%")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_175", "\\E2* Heheheheh./")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_176", "* You just can't&  get enough^1, can you!?/")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_177", "\\E4* .../")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_178", "\\E6* How many more&  times will you&  kill her?/")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_179", "* Ha ha ha ha ha.../")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_180", "\\E4*\\TF You disgusting animal./")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_181", "* You didn't even&  TRY to spare her./%%")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_186", "* Wow^1, you really&  can't get enough./")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_187", "\\E5* You kind of remind&  me of myself./%%")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_195", "* Wow^1, you're utterly&  repulsive./")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_196", "\\E6* You spared her life.../")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_197", "* Then you decided that&  just wasn't interesting&  enough for you./")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_198", "\\E7* So you murdered her&  just to see what&  would happen./")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_199", "\\E5* You killed her out&  of boredom./")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_200", "* Truly disgusting.../%%")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_203", "* Hee hee.../")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_204", "\\E4* You naive idiot./")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_205", "* Do you think you&  are the only one&  with that power?/")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_206", "\\E4* The power to reshape&  the world.../")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_207", "* Purely by your&  own determination./")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_208", "* The ability to&  play God!/")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_209", "* The ability to \"SAVE.\"/")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_210", "\\E0* I thought I was&  the only one with&  that power^1. But.../")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_211", "\\E1* I can't SAVE anymore./")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_212", "* Apparently YOUR&  desires for this&  world override MINE./")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_213", "\\E5* Well well./")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_214", "* Enjoy that power&  while you can./")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_215", "\\E4* I'll be watching./%%")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_226", "* Hahaha.../")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_227", "\\E1* You're not really human^1,&  are you?/")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_228", "\\E1* No.^2 You're empty inside^2.&* \\E4Just like me^1.&* In fact.../")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_229", "\\E8* You're \\[C],^1 right?/")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_230", "\\E0* We're still inseparable^1,&  after all these years.../")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_231", "* Listen^1.&* I have a plan to become&  all powerful./")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_232", "\\E2* Even more powerful than&  you and your stolen&  soul./")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_233", "\\E4* Let's destroy everything&  in this wretched world./")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_234", "\\E6* Everyone^1, everything in&  these \\E7worthless&  memories.../")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_235", "\\E5* Let's turn 'em all&  to dust./%%")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_245", "* Clever.&* Verrrryyy clever./")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_246", "* You think you're really&  smart^1, don't you?/")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_247", "\\E0* In this world^1, it's kill&  or be killed./")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_248", "\\E1* So you were able to&  play by your own&  rules./")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_249", "*\\E4 You spared the life&  of a single person./")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_250", "* Hee hee hee.../")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_253", "* Froggit^1, Whimsun^1.&* Vegetoid^1, Loox^1.&* Migosp^1, Moldsmal./")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_254", "* Think about those names./")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_255", "* Do you think any of&  those monsters have&  families?/")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_256", "* Do you think any of&  them have friends?/")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_257", "\\E6* Each one could have&  been someone else's&  Toriel./")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_258", "\\E7* Selfish brat./")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_259", "* Somebody is dead&  because of you./%%")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_264", "\\E2* I bet you feel&  really great./")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_265", "* You didn't kill&  anybody this time./")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_266", "\\E0* But what will you&  do if you meet a&  relentless killer?/")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_267", "* You'll die and you'll&  die and you'll&  die./")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_268", "\\E1* Until you tire of&  trying./")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_269", "\\E0* What will you do&  then?/")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_270", "\\E5* Will you kill&  out of frustration?/")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_271", "* Or will you give&  up entirely on&  this world.../")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_272", "\\E4* ... and let ME inherit&  the power to control&  it?/")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_273", "\\E5* I am the prince of&  this world's future./")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_274", "* Don't worry^1, my little&  monarch^1, my plan&  isn't regicide./")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_275", "* This is SO much&  more interesting./%%")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_283", "* But don't act so&  cocky./")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_284", "* I know what you did./")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_285", "\\E6* You murdered her./")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_286", "\\E7* And then you went&  back^1, because you&  regretted it./")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_287", "* Ha ha ha ha.../")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_288", "\\E5* You naive idiot./")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_289", "* Do you think you&  are the only one&  with that power?/")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_290", "\\E4* The power to reshape&  the world.../")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_291", "* Purely by your&  own determination./")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_292", "* The ability to&  play God!/")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_293", "* The ability to \"SAVE.\"/")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_294", "\\E0* I thought I was&  the only one with&  that power^1. But.../")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_295", "\\E1* I can't SAVE anymore./")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_296", "* Apparently YOUR&  desires for this&  world override MINE./")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_297", "\\E5* Well well./")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_298", "* Enjoy that power&  while you can./")
	ds_map_add(global.text_data_en, "obj_floweytrigger2_299", "\\E4* I'll be watching./%%")
	ds_map_add(global.text_data_en, "obj_forcedright_142", "* What's that smell^1?&* (Where's that smell?)/")
	ds_map_add(global.text_data_en, "obj_forcedright_143", "* If you're a smell^1...&* (... identify yoursmellf!)/%%")
	ds_map_add(global.text_data_en, "obj_forcedright_175", "* Hmmm..^1.&* Here's that weird smell.../")
	ds_map_add(global.text_data_en, "obj_forcedright_176", "* It makes me want to&  eliminate./")
	ds_map_add(global.text_data_en, "obj_forcedright_177", "* (... Eliminate YOU!)/%%")
	ds_map_add(global.text_data_en, "obj_forcedright_220", "* Dogs can pet other dogs??^1?&* (A new world has opened&  up for us...)/")
	ds_map_add(global.text_data_en, "obj_forcedright_221", "* Thanks^1, weird puppy!/%%")
	ds_map_add(global.text_data_en, "obj_forcedright_224", "* Weird smells can bring&  good things..^1.&* (Friendly fun fetch!)/")
	ds_map_add(global.text_data_en, "obj_forcedright_225", "* Thanks^1, weird smell^1!&* (It sure was fun to&  \"stick\" together!)/%%")
	ds_map_add(global.text_data_en, "obj_friendc_event_115", "  Frisk!/")
	ds_map_add(global.text_data_en, "obj_friendc_event_116", "  This is all just&  a bad dream...!/")
	ds_map_add(global.text_data_en, "obj_friendc_event_117", "  Please^1, wake up...!/%%")
	ds_map_add(global.text_data_en, "obj_friendc_event_181", "* Oh^1!&* You are awake^1!&* Thank goodness!/")
	ds_map_add(global.text_data_en, "obj_friendc_event_183", "* W-we were so&  worried...!/")
	ds_map_add(global.text_data_en, "obj_friendc_event_184", "* It felt like you&  were out forever!/")
	ds_map_add(global.text_data_en, "obj_friendc_event_186", "* Yeah^1! Any longer and&  I would have freaked&  out!/")
	ds_map_add(global.text_data_en, "obj_friendc_event_187", "\\E2* Tell us next time&  you decide to take&  a nap^1, okay!?/")
	ds_map_add(global.text_data_en, "obj_friendc_event_189", "* yeah^1.&* you made papyrus cry&  like a baby./")
	ds_map_add(global.text_data_en, "obj_friendc_event_191", "WHAT!^1!&I DIDN'T CRY!!^1!&I DON'T CRY!!/")
	ds_map_add(global.text_data_en, "obj_friendc_event_192", "\\E3I JUST..^1.&CAUGHT SOMETHING&IN MY EYE./")
	ds_map_add(global.text_data_en, "obj_friendc_event_194", "* what did you catch?/")
	ds_map_add(global.text_data_en, "obj_friendc_event_196", "TEARS!!!/")
	ds_map_add(global.text_data_en, "obj_friendc_event_198", "* Now^1, now./")
	ds_map_add(global.text_data_en, "obj_friendc_event_199", "* The important part&  is that Frisk is&  all right./")
	ds_map_add(global.text_data_en, "obj_friendc_event_200", "* Here^1, Frisk^1.&* Why not drink some&  tea?/")
	ds_map_add(global.text_data_en, "obj_friendc_event_201", "* It'll make you feel&  better./")
	ds_map_add(global.text_data_en, "obj_friendc_event_203", "* Errr..^1.&* How about we give&  them space^1, first?/")
	ds_map_add(global.text_data_en, "obj_friendc_event_204", "\\E2* They must be very&  exhausted./")
	ds_map_add(global.text_data_en, "obj_friendc_event_205", "\\E8* Though^1, from what^1,&  I am not certain./")
	ds_map_add(global.text_data_en, "obj_friendc_event_206", "\\E1* Frisk..^1.&* We do not remember&  exactly what happened./")
	ds_map_add(global.text_data_en, "obj_friendc_event_207", "\\E1* There was a flower..^1.&* And then^1, everything&  went white./")
	ds_map_add(global.text_data_en, "obj_friendc_event_208", "\\E0* But now the barrier&  is gone./")
	ds_map_add(global.text_data_en, "obj_friendc_event_209", "* When you are ready^1,&  we will all return&  to the surface./")
	ds_map_add(global.text_data_en, "obj_friendc_event_210", "* It seems the door to&  the east will lead&  us there now./")
	ds_map_add(global.text_data_en, "obj_friendc_event_211", "\\E2* But before then.../")
	ds_map_add(global.text_data_en, "obj_friendc_event_212", "\\E0* Perhaps you might&  want to take a&  walk?/")
	ds_map_add(global.text_data_en, "obj_friendc_event_213", "* You can say goodbye&  to all of your&  wonderful friends./")
	ds_map_add(global.text_data_en, "obj_friendc_event_214", "* Do as you wish^1.&* We will all wait&  for you here./%%")
	ds_map_add(global.text_data_en, "obj_friendscene_182", "\\E4What a miserable&creature^1, torturing&such a poor^1,&innocent, youth.../")
	ds_map_add(global.text_data_en, "obj_friendscene_183", "\\E0Do not be afraid^1,&my child./")
	ds_map_add(global.text_data_en, "obj_friendscene_184", "\\E0It is I^1, TORIEL^1,&your friend and&guardian./")
	ds_map_add(global.text_data_en, "obj_friendscene_185", "\\E1At first^1, I thought&I would let you&make your journey&alone.../")
	ds_map_add(global.text_data_en, "obj_friendscene_186", "\\E4But I could not&stop worrying about&you./")
	ds_map_add(global.text_data_en, "obj_friendscene_187", "\\E4Your adventure must&have been so&treacherous./")
	ds_map_add(global.text_data_en, "obj_friendscene_188", "\\E2... and ultimately^1,&it would burden you&with a horrible&choice./")
	ds_map_add(global.text_data_en, "obj_friendscene_189", "To leave this place^1,&you would have to&take the life of&another person./")
	ds_map_add(global.text_data_en, "obj_friendscene_190", "\\E2You would have to&defeat ASGORE./")
	ds_map_add(global.text_data_en, "obj_friendscene_191", "\\E4However..^1.&I realized.../")
	ds_map_add(global.text_data_en, "obj_friendscene_192", "\\E1I cannot allow that./")
	ds_map_add(global.text_data_en, "obj_friendscene_193", "It is not right to&sacrifice someone&simply to let&someone leave here./")
	ds_map_add(global.text_data_en, "obj_friendscene_194", "\\E4Is that not what&I have been trying to&prevent this&whole time?/")
	ds_map_add(global.text_data_en, "obj_friendscene_195", "\\E1So^1, for now^1, let&us suspend this&battle./")
	ds_map_add(global.text_data_en, "obj_friendscene_196", "\\E4As terrible as&ASGORE is.../")
	ds_map_add(global.text_data_en, "obj_friendscene_197", "\\E0He deserves mercy^1,&too./%%")
	ds_map_add(global.text_data_en, "obj_friendscene_226", "Tori..^1.&You&came&back...!/%%")
	ds_map_add(global.text_data_en, "obj_friendscene_236", "\\E5Do not \"Tori\" me^1,&Dreemurr!/")
	ds_map_add(global.text_data_en, "obj_friendscene_237", "\\E2You pathetic whelp./")
	ds_map_add(global.text_data_en, "obj_friendscene_238", "If you really wanted&to free our kind.../")
	ds_map_add(global.text_data_en, "obj_friendscene_239", "You could have gone&through the barrier&after you got ONE&SOUL.../")
	ds_map_add(global.text_data_en, "obj_friendscene_240", "... taken six SOULs&from the humans^1, then&come back and freed&everyone peacefully./")
	ds_map_add(global.text_data_en, "obj_friendscene_241", "\\E5But instead^1, you made&everyone live in&despair.../")
	ds_map_add(global.text_data_en, "obj_friendscene_242", "Because you would&rather wait here^1,&meekly hoping another&human never comes./%%")
	ds_map_add(global.text_data_en, "obj_friendscene_271", ".../")
	ds_map_add(global.text_data_en, "obj_friendscene_272", "... Tori.../")
	ds_map_add(global.text_data_en, "obj_friendscene_273", "You're right.../")
