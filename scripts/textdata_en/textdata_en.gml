	ds_map_add(global.text_data_en, "obj_sansb_457", "\\E3are you ready?/")
	ds_map_add(global.text_data_en, "obj_sansb_458", "\\Xs\\E5urvive THIS^1, and&i'll show you my\\R &special attack\\X!/%%")
