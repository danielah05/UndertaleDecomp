	ds_map_add(global.text_data_en, "obj_readable_room5_1", "* ('You must be this tall and&  jpeg artifacted to play&  Poker.')/%")
	ds_map_add(global.text_data_en, "obj_readable_room5_2", "* (It's a poker table.^1)&* (It's so tall there's no&  way you can ever play.)/%")
	ds_map_add(global.text_data_en, "obj_readable_room5_3", "* (It's some kind of statue&  advertising fried dog food.)/")
	ds_map_add(global.text_data_en, "obj_readable_room5_4", "* (Felt as if you put something&  into the dog's hands...)/")
