	ds_map_add(global.text_data_en, "item_desc_0_0", "* If you are reading this,&  I messed up somehow./%")
	ds_map_add(global.text_data_en, "item_desc_1_0", "* \"Monster Candy\" - Heals 10 HP&* Has a distinct,^1 &  non-licorice flavor./%")
	ds_map_add(global.text_data_en, "item_desc_2_0", "* \"Croquet Roll\" - Heals 15 HP&* Fried dough traditionally&  served with a mallet./%")
	ds_map_add(global.text_data_en, "item_desc_3_0", "* \"Stick\" - Weapon AT 0&* Its bark is worse than&  its bite./%")
	ds_map_add(global.text_data_en, "item_desc_4_0", "* \"Bandage\" - Heals 10 HP&* It has already been used&  several times./%")
	ds_map_add(global.text_data_en, "item_desc_5_0", "* \"Rock Candy\" - Heals 1 HP&* Here is a recipe to make&  this at home:/")
	ds_map_add(global.text_data_en, "item_desc_5_1", "* 1. Find a rock/%")
	ds_map_add(global.text_data_en, "item_desc_6_0", "* \"Pumpkin Rings\" - Heals 8 HP&* A small pumpkin cooked&  like onion rings./%")
	ds_map_add(global.text_data_en, "item_desc_7_0", "* \"Spider Donut\" - Heals 12 HP&* A donut made with Spider&  Cider in the batter./%")
	ds_map_add(global.text_data_en, "item_desc_8_0", "* \"Stoic Onion\" - Heals 5 HP&* Even eating it raw^1, the&  tears just won't come./%")
	ds_map_add(global.text_data_en, "item_desc_9_0", "* \"Ghost Fruit\" - Heals 16 HP&* If eaten^1, it will never&  pass to the other side./%")
	ds_map_add(global.text_data_en, "item_desc_10_0", "* \"Spider Cider\" - Heals 24 HP&* Made with whole spiders^1,&  not just the juice./%")
	ds_map_add(global.text_data_en, "item_desc_11_0", "* \"Butterscotch Pie\" - All HP&* Butterscotch-cinnamon&  pie^1, one slice./%")
	ds_map_add(global.text_data_en, "item_desc_12_0", "* \"Faded Ribbon\" - Armor DF 3&* If you're cuter^1, monsters&  won't hit you as hard./%")
	ds_map_add(global.text_data_en, "item_desc_13_0", "* \"Toy Knife\" - Weapon AT 3&* Made of plastic.&* A rarity nowadays./%")
	ds_map_add(global.text_data_en, "item_desc_14_0", "* \"Tough Glove\" - Weapon AT 5&* A worn pink leather glove^1.&* For five-fingered folk./%")
	ds_map_add(global.text_data_en, "item_desc_15_0", "* \"Manly Bandanna\" - Armor DF 7&* It has seen some wear.&* It has abs drawn on it./%")
	ds_map_add(global.text_data_en, "item_desc_16_0", "* \"Snowman Piece\" - Heals 45 HP&* Please take this to the&  ends of the earth./%")
	ds_map_add(global.text_data_en, "item_desc_17_0", "* \"Nice Cream\" - Heals 15 HP&* Instead of a joke,^1 the&  wrapper says something nice./%")
	ds_map_add(global.text_data_en, "item_desc_18_0", "* \"Puppydough Icecream\"&* Heals 28 HP^1.&* Made by young pups./%")
	ds_map_add(global.text_data_en, "item_desc_19_0", "* \"Bisicle\" - Heals 11 HP&* It's a two-pronged popsicle^1,&  so you can eat it twice./%")
	ds_map_add(global.text_data_en, "item_desc_20_0", "* \"Unisicle\" - Heals 11 HP&* It's a SINGLE-pronged popsicle^1.&* Wait^1, that's just normal.../%")
	ds_map_add(global.text_data_en, "item_desc_21_0", "* \"Cinnamon Bunny\" - Heals 22 HP&* A cinnamon roll in the shape&  of a bunny./%%")
	ds_map_add(global.text_data_en, "item_desc_22_0", "* \"Temmie Flakes\" - Heals 2 HP&* It's just torn up pieces&  of colored construction paper./%%")
	ds_map_add(global.text_data_en, "item_desc_23_0", "* \"Abandoned Quiche\" Heals 34 HP&* A psychologically damaged&  spinach egg pie./%%\"")
	ds_map_add(global.text_data_en, "item_desc_24_0", "* \"Old Tutu\" - Armor DF 10&* Finally^1, a protective piece&  of armor./%%")
	ds_map_add(global.text_data_en, "item_desc_25_0", "* \"Ballet Shoes\" - Wpn AT 7&* These used shoes make you&  feel incredibly dangerous./%%")
	ds_map_add(global.text_data_en, "item_desc_26_0", "* \"Punch Card\" - Battle Item&* Use to make punching attacks&  stronger in one battle./")
	ds_map_add(global.text_data_en, "item_desc_26_1", "* Use outside of battle to&  look at the card./%%")
	ds_map_add(global.text_data_en, "item_desc_27_0", "* \"Annoying Dog\" - Dog&* A little white dog^1.&* It's fast asleep.../%%")
	ds_map_add(global.text_data_en, "item_desc_28_0", "* \"Dog Salad\" - Heals ?? HP&* Recovers HP.&* (Hit Poodles.)/%%")
	ds_map_add(global.text_data_en, "item_desc_29_0", "* \"Dog Residue\" - Dog Item&* Shiny trail left&  behind by a dog./%%")
	ds_map_add(global.text_data_en, "item_desc_30_0", "* \"Dog Residue\" - Dog Item&* Dog-shaped husk shed&  from a dog's carapace./%%")
	ds_map_add(global.text_data_en, "item_desc_31_0", "* \"Dog Residue\" - Dog Item&* Dirty dishes left&  unwashed by a dog./%%")
	ds_map_add(global.text_data_en, "item_desc_32_0", "* \"Dog Residue\" - Dog Item&* Glowing crystals secreted&  by a dog./%%")
	ds_map_add(global.text_data_en, "item_desc_33_0", "* \"Dog Residue\" - Dog Item&* Jigsaw puzzle left&  unfinished by a dog./%%")
	ds_map_add(global.text_data_en, "item_desc_34_0", "* \"Dog Residue\" - Dog Item&* Web spun by a dog&  to ensnare prey./%%")
	ds_map_add(global.text_data_en, "item_desc_35_0", "* \"Astronaut Food\" Heals 21 HP&* For feeding a pet&  astronaut./%%")
	ds_map_add(global.text_data_en, "item_desc_36_0", "* \"Instant Noodles\" Heals HP&* Comes with everything you&  need for a quick meal!/%%")
	ds_map_add(global.text_data_en, "item_desc_37_0", "* \"Crab Apple\" Heals 18 HP&* An aquatic fruit that&  resembles a crustacean./%%")
	ds_map_add(global.text_data_en, "item_desc_38_0", "* \"Hot Dog...?\" Heals 20 HP&* The \"meat\" is made of something&  called a \"water sausage.\"/%%")
	ds_map_add(global.text_data_en, "item_desc_39_0", "* \"Hot Cat\" Heals 21 HP&* Like a hot dog^1, but with&  little cat ears on the end./%%")
