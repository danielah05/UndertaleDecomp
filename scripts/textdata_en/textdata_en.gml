	ds_map_add(global.text_data_en, "obj_sansb_478", "you're sparing me?/")
	ds_map_add(global.text_data_en, "obj_sansb_479", "\\E1finally./")
	ds_map_add(global.text_data_en, "obj_sansb_480", "\\E3buddy^1.&pal./")
	ds_map_add(global.text_data_en, "obj_sansb_481", "\\E4i know how hard&it must be.../")
	ds_map_add(global.text_data_en, "obj_sansb_482", "to make that&choice./")
	ds_map_add(global.text_data_en, "obj_sansb_483", "to go back on&everything you've&worked up to./")
	ds_map_add(global.text_data_en, "obj_sansb_484", "\\E0i want you to&know..^1.&i won't let it&go to waste./")
	ds_map_add(global.text_data_en, "obj_sansb_485", "\\M1.../")
	ds_map_add(global.text_data_en, "obj_sansb_486", "\\E3c'mere^1, pal./%%")
