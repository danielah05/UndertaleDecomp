	ds_map_add(global.text_data_en, "SCR_TEXT_4512", "                     Bring &         It's        Them&         great       Back\\C")
	ds_map_add(global.text_data_en, "SCR_TEXT_4513", " ")
	ds_map_add(global.text_data_en, "SCR_TEXT_4519", "* (Glad to hear it.^1)&* (Though^1, I do not know why&  you dislike yellow.)/")
	ds_map_add(global.text_data_en, "SCR_TEXT_4520", "* (You had better hope you do&  not encounter a banana-themed&  monster.)/%%")
	ds_map_add(global.text_data_en, "SCR_TEXT_4524", "* (Huh^1? It's rather inconvenient&  that you changed your mind&  like this.)/")
	ds_map_add(global.text_data_en, "SCR_TEXT_4525", "* (Since I told everyone&  not to use yellow names^1,&  everyone threw theirs out.)/")
	ds_map_add(global.text_data_en, "SCR_TEXT_4526", "* (This is really troubling...^1)&* (Hmmm...)/")
	ds_map_add(global.text_data_en, "SCR_TEXT_4527", "\\W* (Well^1, last year it was&  fashionable to have \\ppink\\W &  names.)/")
	ds_map_add(global.text_data_en, "SCR_TEXT_4528", "* (I think everyone still&  has those in their closets&  somewhere...)/")
	ds_map_add(global.text_data_en, "SCR_TEXT_4529", "* (I'll ask everyone to look.^1)&* (But this is the last time!)/%%")
	ds_map_add(global.text_data_en, "SCR_TEXT_4535", "* Ribbit^1, ribbit...&* (I hope you're satisfied.)/%%")
	ds_map_add(global.text_data_en, "SCR_TEXT_4540", "* \\YNAPSTABLOOK22 has sent you&  a friend request.\\W /")
	ds_map_add(global.text_data_en, "SCR_TEXT_4541", "* Accept the request?& &         Accept      Reject\\C")
	ds_map_add(global.text_data_en, "SCR_TEXT_4542", " ")
	ds_map_add(global.text_data_en, "SCR_TEXT_4547", "* (It seems to have already&  rejected itself...)/%%")
	ds_map_add(global.text_data_en, "SCR_TEXT_4588", "* hey^1.&* i heard you're going&  to the core./")
	ds_map_add(global.text_data_en, "SCR_TEXT_4589", "\\E0* how about grabbing some&  dinner with me first?&  Yeah        I'm busy \\C")
