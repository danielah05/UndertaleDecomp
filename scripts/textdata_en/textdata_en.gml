	ds_map_add(global.text_data_en, "obj_labtv_event_287", "* Psssst^1.&* Gorey^1, wake up. \\Ta/")
	ds_map_add(global.text_data_en, "obj_labtv_event_288", "* Mmm^1?&* What is it^1, dear?/")
	ds_map_add(global.text_data_en, "obj_labtv_event_289", "* ... err^1, and why do you&  have that video camera? \\TT/")
