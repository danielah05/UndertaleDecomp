	ds_map_add(global.text_data_en, "SCR_TEXT_4477", "* Ribbit^1, ribbit.&* (I have heard you are quite&  merciful^1, for a human...)/")
	ds_map_add(global.text_data_en, "SCR_TEXT_4478", "\\W* (Surely you know by now a&  monster wears a \\YYELLOW\\W name&  when you can \\YSPARE\\W it.)/")
	ds_map_add(global.text_data_en, "SCR_TEXT_4479", "* (What do you think of that?)&         Very        It's&         Helpful     Bad\\C")
	ds_map_add(global.text_data_en, "SCR_TEXT_4480", " ")
	ds_map_add(global.text_data_en, "SCR_TEXT_4486", "* (It is rather helpful.^1)&* (Remember^1, sparing is just&  saying you won't fight.)/")
	ds_map_add(global.text_data_en, "SCR_TEXT_4487", "* (Maybe one day^1, you'll&  have to do it even if&  their name isn't yellow.)/%%")
	ds_map_add(global.text_data_en, "SCR_TEXT_4491", "* (Really^1? Then^1, I'll tell all&  of my friends to tell&  their friends' friends...)/")
	ds_map_add(global.text_data_en, "SCR_TEXT_4492", "* (Never use yellow names.)&* (How about that?)/")
	ds_map_add(global.text_data_en, "SCR_TEXT_4493", "         Keep        No more&         Yellow      Yellow&         Names       Names\\C")
	ds_map_add(global.text_data_en, "SCR_TEXT_4494", " ")
	ds_map_add(global.text_data_en, "SCR_TEXT_4501", "* (OK^1, they will still&  use yellow names.)/%%")
