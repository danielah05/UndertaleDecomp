	ds_map_add(global.text_data_en, "obj_truechara_111", "Thank you./")
	ds_map_add(global.text_data_en, "obj_truechara_112", "Your power awakened&me from death./")
	ds_map_add(global.text_data_en, "obj_truechara_113", "My \"human soul.\"/")
	ds_map_add(global.text_data_en, "obj_truechara_114", "My \"determination.\"/")
	ds_map_add(global.text_data_en, "obj_truechara_115", "They were not mine^1,&but YOURS./")
	ds_map_add(global.text_data_en, "obj_truechara_116", "At first^1, I&was so confused./")
	ds_map_add(global.text_data_en, "obj_truechara_117", "Our plan had failed^1,&hadn't it?/")
	ds_map_add(global.text_data_en, "obj_truechara_118", "Why was I brought&back to life?/")
	ds_map_add(global.text_data_en, "obj_truechara_119", ".../")
	ds_map_add(global.text_data_en, "obj_truechara_120", "You./")
	ds_map_add(global.text_data_en, "obj_truechara_121", "With your guidance./")
	ds_map_add(global.text_data_en, "obj_truechara_122", "I realized the purpose&of my reincarnation./")
	ds_map_add(global.text_data_en, "obj_truechara_123", "Power./")
	ds_map_add(global.text_data_en, "obj_truechara_124", "Together^1, we eradicated&the enemy and became&strong./")
	ds_map_add(global.text_data_en, "obj_truechara_125", "HP. ATK. DEF.&GOLD. EXP. LV./")
	ds_map_add(global.text_data_en, "obj_truechara_126", "Every time a number&increases^1, that&feeling.../")
	ds_map_add(global.text_data_en, "obj_truechara_127", "That's me./")
	ds_map_add(global.text_data_en, "obj_truechara_128", "\"\\[C].\"/")
	ds_map_add(global.text_data_en, "obj_truechara_129", "Now./")
	ds_map_add(global.text_data_en, "obj_truechara_130", "Now, we have reached&the absolute./")
	ds_map_add(global.text_data_en, "obj_truechara_131", "There is nothing&left for us here./")
	ds_map_add(global.text_data_en, "obj_truechara_132", "Let us erase this&pointless world^1, and&move on to the next./%%")
	ds_map_add(global.text_data_en, "obj_truechara_136", "\"\\[C].\"/")
	ds_map_add(global.text_data_en, "obj_truechara_137", "The demon that comes&when people call&its name./")
	ds_map_add(global.text_data_en, "obj_truechara_138", "It doesn't matter when./")
	ds_map_add(global.text_data_en, "obj_truechara_139", "It doesn't matter where./")
	ds_map_add(global.text_data_en, "obj_truechara_140", "Time after time,&I will appear./")
	ds_map_add(global.text_data_en, "obj_truechara_141", "And, with your help./")
	ds_map_add(global.text_data_en, "obj_truechara_142", "We will eradicate the&enemy and become&strong./")
	ds_map_add(global.text_data_en, "obj_truechara_143", "HP. ATK. DEF.&GOLD. EXP. LV./")
	ds_map_add(global.text_data_en, "obj_truechara_144", "Every time a number&increases^1, that&feeling.../")
	ds_map_add(global.text_data_en, "obj_truechara_145", "That's me./")
	ds_map_add(global.text_data_en, "obj_truechara_146", "\"\\[C].\"/")
	ds_map_add(global.text_data_en, "obj_truechara_147", ".../")
	ds_map_add(global.text_data_en, "obj_truechara_148", "But./")
	ds_map_add(global.text_data_en, "obj_truechara_149", "You and I are not&the same^1, are we?/")
	ds_map_add(global.text_data_en, "obj_truechara_150", "This SOUL resonates&with a strange&feeling./")
	ds_map_add(global.text_data_en, "obj_truechara_151", "There is a reason&you continue to&recreate this world./")
	ds_map_add(global.text_data_en, "obj_truechara_152", "There is a reason&you continue to&destroy it./")
	ds_map_add(global.text_data_en, "obj_truechara_153", "You./")
	ds_map_add(global.text_data_en, "obj_truechara_154", "You are wracked with&a perverted&sentimentality./")
	ds_map_add(global.text_data_en, "obj_truechara_155", "Hmm./")
	ds_map_add(global.text_data_en, "obj_truechara_156", "I cannot understand&these feelings&any more./")
	ds_map_add(global.text_data_en, "obj_truechara_157", "Despite this./")
	ds_map_add(global.text_data_en, "obj_truechara_158", "I feel obligated to&suggest./")
	ds_map_add(global.text_data_en, "obj_truechara_159", "Should you choose to&create this world&once more./")
	ds_map_add(global.text_data_en, "obj_truechara_160", "Another path would&be better suited./")
	ds_map_add(global.text_data_en, "obj_truechara_161", "Now, partner./")
