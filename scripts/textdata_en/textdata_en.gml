	ds_map_add(global.text_data_en, "SCR_TEXT_318", "\\E2* To make progress here,^1 &  you will need to trigger&  several switches./")
	ds_map_add(global.text_data_en, "SCR_TEXT_319", "* Do not worry,^1 I have &  labelled the ones that&  you need to flip./%")
	ds_map_add(global.text_data_en, "SCR_TEXT_322", "* Do not worry,^1 I have &  labelled the ones that&  you need to flip./")
	ds_map_add(global.text_data_en, "SCR_TEXT_323", "* ... eh^1?&* It seems that the&  labelling has worn away./")
	ds_map_add(global.text_data_en, "SCR_TEXT_324", "* Oh dear./")
	ds_map_add(global.text_data_en, "SCR_TEXT_325", "* This might be far more&  challenging than I&  anticipated.../%%")
	ds_map_add(global.text_data_en, "SCR_TEXT_330", "\\E2* The first switch is over&  on the wall./%")
	ds_map_add(global.text_data_en, "SCR_TEXT_334", "\\E1* Do you need some help..^1?&* Press the switch on the   wall./")
