	ds_map_add(global.text_data_en, "obj_mewmew_npc_76", "* (The Animatronic Limited&  Edition Mew Mew Machine&  was shattered into pieces...)/")
	ds_map_add(global.text_data_en, "obj_mewmew_npc_77", "* YOU^1! I had finally found&  the perfect body...!!!/")
	ds_map_add(global.text_data_en, "obj_mewmew_npc_78", "* I even came to a secluded&  place to fuse in PEACE^1,&  but.../")
	ds_map_add(global.text_data_en, "obj_mewmew_npc_79", "* I guess I made an ENEMY!!!/")
	ds_map_add(global.text_data_en, "obj_mewmew_npc_80", "* Just WAIT^1! After I repair&  this body^1, you're REALLY&  going to suffer!!!/")
	ds_map_add(global.text_data_en, "obj_mewmew_npc_81", "* Someday^1.&* Someday^1!&* SOMEDAY!!!/%%")
	ds_map_add(global.text_data_en, "obj_mewmew_npc_85", "* Huh...^1?&* Such..^1. such kindness...!/")
	ds_map_add(global.text_data_en, "obj_mewmew_npc_86", "* It's..^1. making me FEEL&  something!/")
	ds_map_add(global.text_data_en, "obj_mewmew_npc_87", "* It may be a while before I&  can truly fuse with this&  body./")
	ds_map_add(global.text_data_en, "obj_mewmew_npc_88", "* But^1, I think this is&  a start...!/")
