	ds_map_add(global.text_data_en, "obj_papyrus8_215", "NO.../")
	ds_map_add(global.text_data_en, "obj_papyrus8_216", "NO^1, THIS IS ALL&WRONG!/")
	ds_map_add(global.text_data_en, "obj_papyrus8_217", "I CAN'T BE YOUR&FRIEND!!!/")
	ds_map_add(global.text_data_en, "obj_papyrus8_218", "YOU ARE A HUMAN!/")
	ds_map_add(global.text_data_en, "obj_papyrus8_219", "I MUST CAPTURE&YOU!!!/")
	ds_map_add(global.text_data_en, "obj_papyrus8_220", "THEN^1, I CAN FULFILL&MY LIFELONG DREAM!!!/")
