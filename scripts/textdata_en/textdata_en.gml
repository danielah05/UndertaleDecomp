	ds_map_add(global.text_data_en, "obj_truechara_186", "Right^1. You are a&great partner./")
	ds_map_add(global.text_data_en, "obj_truechara_187", "We'll be together&forever^1, won't we?/%%")
	ds_map_add(global.text_data_en, "obj_truechara_209", "No...?/")
	ds_map_add(global.text_data_en, "obj_truechara_210", "Hmm.../")
	ds_map_add(global.text_data_en, "obj_truechara_211", "How curious./")
	ds_map_add(global.text_data_en, "obj_truechara_212", "You must have&misunderstood./")
	ds_map_add(global.text_data_en, "obj_truechara_213", "SINCE WHEN WERE YOU&THE ONE IN CONTROL?/%%")
	ds_map_add(global.text_data_en, "obj_truechara_217", "No...?/")
	ds_map_add(global.text_data_en, "obj_truechara_218", "Hmm...&This feeling you have./")
	ds_map_add(global.text_data_en, "obj_truechara_219", "This is what I&spoke of./")
	ds_map_add(global.text_data_en, "obj_truechara_220", "Unfortunately,&regarding this.../")
	ds_map_add(global.text_data_en, "obj_truechara_221", "YOU MADE YOUR CHOICE&LONG AGO./%%")
