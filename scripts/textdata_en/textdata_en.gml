	ds_map_add(global.text_data_en, "obj_sans_sentry2_141", "\\E3* hey^1, i really&  respect what you did&  back there./")
	ds_map_add(global.text_data_en, "obj_sans_sentry2_142", "\\E2* thanks./%%")
	ds_map_add(global.text_data_en, "obj_sans_sentry2_306", "* over here^1.&* i know a shortcut./%%")
	ds_map_add(global.text_data_en, "obj_sansb_90", "* Skipped ahead.")
	ds_map_add(global.text_data_en, "obj_sansb_252", "what^1?&you think i'm just&gonna stand there&and take it?/%%")
	ds_map_add(global.text_data_en, "obj_sansb_256", "our reports showed&a massive anomaly&in the timespace&continuum./")
	ds_map_add(global.text_data_en, "obj_sansb_257", "timelines jumping&left and right,&stopping and&starting.../%%")
	ds_map_add(global.text_data_en, "obj_sansb_262", "\\E4until suddenly^1,&everything ends./%%")
	ds_map_add(global.text_data_en, "obj_sansb_266", "\\E4heh heh heh.../")
	ds_map_add(global.text_data_en, "obj_sansb_267", "\\E5that's your fault^1,&isn't it?/%%")
	ds_map_add(global.text_data_en, "obj_sansb_272", "\\E1you can't understand&how this feels./%%")
	ds_map_add(global.text_data_en, "obj_sansb_277", "\\E4knowing that one&day^1, without any&warning.../")
	ds_map_add(global.text_data_en, "obj_sansb_278", "\\E9it's all going to&be reset./%%")
	ds_map_add(global.text_data_en, "obj_sansb_284", "\\E9look^1.&i gave up trying&to go back a long&time ago./%%")
	ds_map_add(global.text_data_en, "obj_sansb_290", "\\E4and getting to the&surface doesn't&really appeal&anymore^1, either./%%")
	ds_map_add(global.text_data_en, "obj_sansb_296", "\\E4cause even if we&do.../")
	ds_map_add(global.text_data_en, "obj_sansb_297", "\\E5we'll just end up&right back here^1,&without any memory&of it^1, right?/%%")
	ds_map_add(global.text_data_en, "obj_sansb_303", "\\E1to be blunt.../")
	ds_map_add(global.text_data_en, "obj_sansb_304", "\\E4it makes it kind&of hard to give&it my all./%%")
	ds_map_add(global.text_data_en, "obj_sansb_310", "\\E1... or is that just&a poor excuse for&being lazy...?/")
	ds_map_add(global.text_data_en, "obj_sansb_311", "\\E3hell if i know./%%")
	ds_map_add(global.text_data_en, "obj_sansb_317", "\\E4all i know is..^1.&seeing what comes&next.../")
	ds_map_add(global.text_data_en, "obj_sansb_318", "\\E9i can't afford not&to care anymore./%%")
	ds_map_add(global.text_data_en, "obj_sansb_334", "\\E9ugh..^1.&that being said.../")
	ds_map_add(global.text_data_en, "obj_sansb_335", "\\E1you^1, uh^1, really&like swinging that&thing around^1,&huh?/")
	ds_map_add(global.text_data_en, "obj_sansb_336", "\\E0.../")
	ds_map_add(global.text_data_en, "obj_sansb_337", "\\E4listen./")
	ds_map_add(global.text_data_en, "obj_sansb_338", "i know you didn't&answer me before^1,&but.../")
	ds_map_add(global.text_data_en, "obj_sansb_339", "\\E4somewhere in&there^1.&i can feel it./")
	ds_map_add(global.text_data_en, "obj_sansb_340", "\\E0there's a glimmer&of a good person&inside of you./")
	ds_map_add(global.text_data_en, "obj_sansb_341", "\\E4the memory of&someone who once&wanted to do the&right thing./")
	ds_map_add(global.text_data_en, "obj_sansb_342", "\\E1someone who^1, in&another time^1,&might have even&been.../")
	ds_map_add(global.text_data_en, "obj_sansb_343", "\\E4a friend?/")
	ds_map_add(global.text_data_en, "obj_sansb_344", "\\E3c'mon^1, buddy./")
	ds_map_add(global.text_data_en, "obj_sansb_345", "\\E0do you remember&me?/")
	ds_map_add(global.text_data_en, "obj_sansb_346", "\\E4please^1, if you're&listening.../")
	ds_map_add(global.text_data_en, "obj_sansb_347", "\\E9let's forget all&this^1, ok?/")
	ds_map_add(global.text_data_en, "obj_sansb_348", "\\E3just lay down&your weapon^1, and.../")
	ds_map_add(global.text_data_en, "obj_sansb_349", "\\E4well^1, my job&will be a lot&easier./%%")
	ds_map_add(global.text_data_en, "obj_sansb_353", "\\E9ugh..^1.&that being said.../")
	ds_map_add(global.text_data_en, "obj_sansb_354", "\\E1you^1, uh^1, really&like swinging that&thing around^1,&huh?/")
	ds_map_add(global.text_data_en, "obj_sansb_355", "\\E0.../")
	ds_map_add(global.text_data_en, "obj_sansb_356", "\\E4listen./")
	ds_map_add(global.text_data_en, "obj_sansb_357", "friendship.../")
	ds_map_add(global.text_data_en, "obj_sansb_358", "\\E3it's really great^1,&right?/")
	ds_map_add(global.text_data_en, "obj_sansb_359", "let's quit fighting./%%")
	ds_map_add(global.text_data_en, "obj_sansb_380", "\\E3welp^1, it was&worth a shot./")
	ds_map_add(global.text_data_en, "obj_sansb_381", "\\E5guess you like&doing things the&hard way^1, huh?/%%")
	ds_map_add(global.text_data_en, "obj_sansb_384", "\\E3woah^1, you look&REALLY pissed off.../")
	ds_map_add(global.text_data_en, "obj_sansb_385", "\\E4heheheh.../")
	ds_map_add(global.text_data_en, "obj_sansb_386", "\\E5did i getcha?/")
	ds_map_add(global.text_data_en, "obj_sansb_387", "\\E4well^1, if you came&back anyway.../")
	ds_map_add(global.text_data_en, "obj_sansb_388", "\\E1i guess that means&we never really&WERE friends^1, huh?/")
	ds_map_add(global.text_data_en, "obj_sansb_389", "\\E4heh./")
	ds_map_add(global.text_data_en, "obj_sansb_390", "\\E9don't tell that&to the other&sans-es^1, ok?/%%")
	ds_map_add(global.text_data_en, "obj_sansb_398", "\\E4sounds strange^1, but&before all this i&was secretly hoping&we could be friends./")
	ds_map_add(global.text_data_en, "obj_sansb_399", "\\E1i always thought the&anomaly was doing&this cause they&were unhappy./")
	ds_map_add(global.text_data_en, "obj_sansb_400", "and when they got&what they wanted^1,&they would stop&all this./%%")
	ds_map_add(global.text_data_en, "obj_sansb_405", "\\E3and maybe all they&needed was..^1.&i dunno./")
	ds_map_add(global.text_data_en, "obj_sansb_406", "\\M1some good food^1,&some bad laughs^1,&some nice friends./%%")
	ds_map_add(global.text_data_en, "obj_sansb_411", "\\E4but that's&ridiculous^1,&right?/")
	ds_map_add(global.text_data_en, "obj_sansb_412", "\\E5yeah^1, you're the&type of person&who won't EVER&be happy./%%")
	ds_map_add(global.text_data_en, "obj_sansb_417", "\\E5you'll keep&consuming timelines&over and over^1,&until.../")
	ds_map_add(global.text_data_en, "obj_sansb_418", "\\E4well./")
	ds_map_add(global.text_data_en, "obj_sansb_419", "\\M1hey./")
	ds_map_add(global.text_data_en, "obj_sansb_420", "\\E3take it from me^1,&kid./")
	ds_map_add(global.text_data_en, "obj_sansb_421", "someday.../")
	ds_map_add(global.text_data_en, "obj_sansb_422", "you gotta learn&when to QUIT./%%")
	ds_map_add(global.text_data_en, "obj_sansb_423", "%%%")
	ds_map_add(global.text_data_en, "obj_sansb_427", "\\E3and that day's&TODAY./%%")
	ds_map_add(global.text_data_en, "obj_sansb_433", "\\E4cause..^1.&y'see../")
	ds_map_add(global.text_data_en, "obj_sansb_434", "\\E1all this fighting&is really tiring&me out./%%")
	ds_map_add(global.text_data_en, "obj_sansb_441", "\\E4and if you keep&pushing me.../")
	ds_map_add(global.text_data_en, "obj_sansb_442", "\\Xt\\E3hen i'll be&forced to use my\\R &special attack\\X./%%")
	ds_map_add(global.text_data_en, "obj_sansb_448", "\\Xy\\E3eah^1, my \\Rspecial&attack\\X.&sound familiar?/")
	ds_map_add(global.text_data_en, "obj_sansb_449", "\\Xw\\E1ell^1, get ready^1.&cause after the&next move^1, i'm&going to \\Ruse it\\X./")
	ds_map_add(global.text_data_en, "obj_sansb_450", "\\E3so^1, if you don't&wanna see it^1, now&would be a good&time to die./%%")
