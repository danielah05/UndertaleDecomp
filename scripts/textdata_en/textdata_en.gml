	ds_map_add(global.text_data_en, "obj_sansb_1018", "* SANS 1 ATK 1 DEF&* The easiest enemy^1.&* Can only deal 1 damage./^")
	ds_map_add(global.text_data_en, "obj_sansb_1079", "it's a beautiful&day outside./")
