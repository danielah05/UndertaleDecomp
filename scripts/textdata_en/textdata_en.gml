	ds_map_add(global.text_data_en, "obj_papyrus8_222", "THAT'S PAPYRUS!!!/")
	ds_map_add(global.text_data_en, "obj_papyrus8_223", "THE NEWEST MEMBER.../")
	ds_map_add(global.text_data_en, "obj_papyrus8_224", "OF THE ROYAL GUARD!/%%")
	ds_map_add(global.text_data_en, "obj_papyrus8_227", "OH..^1.&WHERE COULD THAT&HUMAN HAVE GONE./")
	ds_map_add(global.text_data_en, "obj_papyrus8_228", "... WAIT..^1.&IT'S RIGHT IN&FRONT OF ME!!!/")
	ds_map_add(global.text_data_en, "obj_papyrus8_229", "HELLO^1!&I WAS WORRIED THAT&YOU HAD GOTTEN LOST!/")
	ds_map_add(global.text_data_en, "obj_papyrus8_230", "IT SURE IS A RELIEF&TO KNOW THAT&YOU'RE RIGHT HERE.../")
	ds_map_add(global.text_data_en, "obj_papyrus8_231", "... WAIT A SECOND!!!/")
	ds_map_add(global.text_data_en, "obj_papyrus8_232", "YOU'RE NOT SUPPOSED&TO ESCAPE!!!/")
	ds_map_add(global.text_data_en, "obj_papyrus8_233", "GET BACK THERE!!!/%%")
	ds_map_add(global.text_data_en, "obj_papyrus8_237", "I SUPPOSE THIS PROVES&WHAT I SAID./")
	ds_map_add(global.text_data_en, "obj_papyrus8_238", "WE REALLY ARE BOUND&TOGETHER./")
	ds_map_add(global.text_data_en, "obj_papyrus8_239", "TWO BONES IN THE&SKELETAL ANATOMY OF&FATE!!!/")
	ds_map_add(global.text_data_en, "obj_papyrus8_240", "THAT'S HOW YOU APPEAR&HERE NOW.../")
	ds_map_add(global.text_data_en, "obj_papyrus8_241", "EVEN THOUGH IT'S VERY&IMPOSSIBLE FOR YOU&TO ESCAPE./")
	ds_map_add(global.text_data_en, "obj_papyrus8_242", "AMAZING!!!/")
	ds_map_add(global.text_data_en, "obj_papyrus8_243", "I FEEL A SINCERE.../")
	ds_map_add(global.text_data_en, "obj_papyrus8_244", "WAAAAAIT A SECOND!!/")
	ds_map_add(global.text_data_en, "obj_papyrus8_245", "I STILL HAVE TO&CAPTURE YOU!!!/")
	ds_map_add(global.text_data_en, "obj_papyrus8_246", "WHOOPSIE-DOOPSIE!!/%%")
	ds_map_add(global.text_data_en, "obj_papyrus8_252", "BACK AGAIN^1, EH?/")
	ds_map_add(global.text_data_en, "obj_papyrus8_253", "I SUPPOSE IT'S MY&FAULT.../")
	ds_map_add(global.text_data_en, "obj_papyrus8_254", "I TOLD YOU BEFORE&THAT I WOULD MAKE&YOU SPAGHETTI./")
	ds_map_add(global.text_data_en, "obj_papyrus8_255", "IT'S ONLY NATURAL&THAT YOU WOULD&WANT TO SEE ME.../")
	ds_map_add(global.text_data_en, "obj_papyrus8_256", "IN THE DIRE HOPE&THAT I WOULD MAKE&YOU SOME./")
	ds_map_add(global.text_data_en, "obj_papyrus8_257", "WELL..^1. I UNDERSTAND./")
	ds_map_add(global.text_data_en, "obj_papyrus8_258", "PAPYRUS IS HUNGRY^1,&TOO!/")
	ds_map_add(global.text_data_en, "obj_papyrus8_259", "HUNGRY FOR JUSTICE!/%%")
	ds_map_add(global.text_data_en, "obj_papyrus8_271", "HALT^1, HUMAN!/%%")
	ds_map_add(global.text_data_en, "obj_papyrus8_386", "I WAS NEVER ONE&  TO BEPIS./%%")
	ds_map_add(global.text_data_en, "obj_papyrus8_414", "\\E1OH^1, I ALMOST&FORGOT TO TELL&YOU.../")
	ds_map_add(global.text_data_en, "obj_papyrus8_415", "TO REACH THE EXIT^1,&YOU WILL HAVE&TO PASS.../")
	ds_map_add(global.text_data_en, "obj_papyrus8_416", "\\WTHROUGH \\YTHE KING\\W'S&CASTLE./")
	ds_map_add(global.text_data_en, "obj_papyrus8_419", "TO REACH THE EXIT^1,&YOU WILL HAVE&TO MEET.../")
	ds_map_add(global.text_data_en, "obj_papyrus8_420", "THE KING./")
	ds_map_add(global.text_data_en, "obj_papyrus8_422", "\\YTHE KING OF&ALL MONSTERS.../")
	ds_map_add(global.text_data_en, "obj_papyrus8_423", "HE IS.../")
	ds_map_add(global.text_data_en, "obj_papyrus8_424", "... WELL.../%%")
	ds_map_add(global.text_data_en, "obj_papyrus8_441", "\\E0HE'S A BIG FUZZY&PUSHOVER!!!/")
	ds_map_add(global.text_data_en, "obj_papyrus8_442", "EVERYBODY LOVES&THAT GUY./")
	ds_map_add(global.text_data_en, "obj_papyrus8_443", "I AM CERTAIN IF&YOU JUST SAY.../")
	ds_map_add(global.text_data_en, "obj_papyrus8_444", "\"EXCUSE ME^1, MR.&DREEMURR..^1. CAN I&PLEASE GO HOME?\"/")
	ds_map_add(global.text_data_en, "obj_papyrus8_445", "HE'LL GUIDE YOU&RIGHT TO THE&BARRIER HIMSELF!/")
	ds_map_add(global.text_data_en, "obj_papyrus8_446", "ANYWAY!!^1!&THAT'S ENOUGH&TALKING!!!/")
	ds_map_add(global.text_data_en, "obj_papyrus8_447", "I'LL BE AT HOME&BEING A COOL&FRIEND!!!/")
	ds_map_add(global.text_data_en, "obj_papyrus8_448", "FEEL FREE TO&COME BY AND&HANG OUT!!!/")
	ds_map_add(global.text_data_en, "obj_papyrus8_449", "FEEL FREE TO&COME BY AND HAVE&THAT DATE!/")
	ds_map_add(global.text_data_en, "obj_papyrus8_450", "NYEH HEH HEH HEH&HEH HEH HEH!!!/%%")
	ds_map_add(global.text_data_en, "obj_papyrus8_515", "HEY^1, QUIT MOVING&WHILE I'M TALKING&TO YOU!/")
	ds_map_add(global.text_data_en, "obj_papyrus8_516", "I^1, THE GREAT PAPYRUS^1,&HAVE SOME THINGS&TO SAY./")
	ds_map_add(global.text_data_en, "obj_papyrus8_517", "FIRST^1: YOU'RE A&FREAKING WEIRDO!/")
	ds_map_add(global.text_data_en, "obj_papyrus8_518", "NOT ONLY DO YOU&NOT LIKE PUZZLES./")
	ds_map_add(global.text_data_en, "obj_papyrus8_519", "BUT THE WAY YOU&SHAMBLE ABOUT FROM&PLACE TO PLACE.../")
	ds_map_add(global.text_data_en, "obj_papyrus8_520", "THE WAY YOUR HANDS&ARE ALWAYS COVERED&IN DUSTY POWDER./")
	ds_map_add(global.text_data_en, "obj_papyrus8_521", "IT FEELS.../")
	ds_map_add(global.text_data_en, "obj_papyrus8_522", "LIKE YOUR LIFE IS&GOING DOWN A&DANGEROUS PATH./")
	ds_map_add(global.text_data_en, "obj_papyrus8_523", "HOWEVER!/")
	ds_map_add(global.text_data_en, "obj_papyrus8_524", "I^1, PAPYRUS^1, SEE&GREAT POTENTIAL&WITHIN YOU!/")
	ds_map_add(global.text_data_en, "obj_papyrus8_525", "EVERYONE CAN BE&A GREAT PERSON&IF THEY TRY!/")
	ds_map_add(global.text_data_en, "obj_papyrus8_526", "AND ME^1, I HARDLY&HAVE TO TRY AT&ALL!!!/")
	ds_map_add(global.text_data_en, "obj_papyrus8_527", "NYEH HEH HEH&HEH HEH HEH!!!/%%")
	ds_map_add(global.text_data_en, "obj_papyrus8_555", "HEY^1, QUIT MOVING!/")
	ds_map_add(global.text_data_en, "obj_papyrus8_556", "THIS IS EXACTLY&WHAT I AM TALKING&ABOUT!/")
	ds_map_add(global.text_data_en, "obj_papyrus8_557", "HUMAN^1! I THINK&YOU ARE IN NEED&OF GUIDANCE!/")
	ds_map_add(global.text_data_en, "obj_papyrus8_558", "SOMEONE NEEDS TO&KEEP YOU ON THE&STRAIGHT AND NARROW!/")
	ds_map_add(global.text_data_en, "obj_papyrus8_559", "BUT WORRY NOT!&I^1, PAPYRUS.../")
	ds_map_add(global.text_data_en, "obj_papyrus8_560", "WILL GLADLY BE&YOUR FRIEND AND&TUTOR!/")
	ds_map_add(global.text_data_en, "obj_papyrus8_561", "I WILL TURN YOUR&LIFE RIGHT&AROUND!!!/%%")
	ds_map_add(global.text_data_en, "obj_papyrus8_590", "I SEE YOU ARE&APPROACHING./")
	ds_map_add(global.text_data_en, "obj_papyrus8_591", "ARE YOU OFFERING A&HUG OF ACCEPTANCE?/")
	ds_map_add(global.text_data_en, "obj_papyrus8_592", "WOWIE!^1! MY LESSONS&ARE ALREADY&WORKING!!/")
	ds_map_add(global.text_data_en, "obj_papyrus8_593", "I^1, PAPYRUS^1,&WELCOME YOU&WITH OPEN ARMS!/%%")
	ds_map_add(global.text_data_en, "obj_papyrus_friendc_76", "\\E3SO^1, ASGORE.../")
	ds_map_add(global.text_data_en, "obj_papyrus_friendc_77", "\\E0HOW ABOUT MAKING&ME A MEMBER OF&THE ROYAL GUARD?/")
	ds_map_add(global.text_data_en, "obj_papyrus_friendc_79", "\\E2* Well^1, Papyrus^1, now&  that the war is&  over.../")
	ds_map_add(global.text_data_en, "obj_papyrus_friendc_80", "\\E0* We might not need&  the Royal Guard&  anymore./")
	ds_map_add(global.text_data_en, "obj_papyrus_friendc_82", "\\E7WHAT!?/")
	ds_map_add(global.text_data_en, "obj_papyrus_friendc_83", "\\E1THEN WHAT WAS&THE POINT OF THE&HUMAN'S QUEST!?/")
	ds_map_add(global.text_data_en, "obj_papyrus_friendc_84", "\\E3THEY JOURNEYED&ALL THAT WAY.../")
	ds_map_add(global.text_data_en, "obj_papyrus_friendc_85", "\\E4AND I'M STILL&NOT A MEMBER OF&THE ROYAL GUARD!?/")
	ds_map_add(global.text_data_en, "obj_papyrus_friendc_86", "\\E0TRULY^1, THIS IS&THE WORST&POSSIBLE ENDING./%%")
	ds_map_add(global.text_data_en, "obj_papyrus_friendc_91", "\\E3IT SEEMS LIKE YOU&REALLY BOTCHED&YOUR QUEST^1, FRISK./")
	ds_map_add(global.text_data_en, "obj_papyrus_friendc_92", "\\E0BUT^1, I^1, THE&GREAT PAPYRUS.../")
	ds_map_add(global.text_data_en, "obj_papyrus_friendc_93", "WILL ALWAYS BE&YOUR FRIEND!!!/")
	ds_map_add(global.text_data_en, "obj_papyrus_friendc_95", "\\E1* no matter how hard&  you try to get&  rid of him./")
	ds_map_add(global.text_data_en, "obj_papyrus_friendc_97", "\\E1HEY!/")
	ds_map_add(global.text_data_en, "obj_papyrus_friendc_98", "\\E0THAT'S TRUE./%%")
	ds_map_add(global.text_data_en, "obj_papyrus_friendc_103", "\\E3FRISK^1, WHY DON'T&YOU TAKE A WALK&AROUND?/")
	ds_map_add(global.text_data_en, "obj_papyrus_friendc_105", "\\E1* frisk^1, why don't you&  fly^1?&* it's faster./")
	ds_map_add(global.text_data_en, "obj_papyrus_friendc_107", "\\E0YEAH^1, FRISK^1, WHY&DON'T YOU FLY?/")
	ds_map_add(global.text_data_en, "obj_papyrus_friendc_109", "\\E1* Hmmm..^1. Flying sounds&  a little dangerous./")
	ds_map_add(global.text_data_en, "obj_papyrus_friendc_111", "\\E3* But they CAN'T fly./")
	ds_map_add(global.text_data_en, "obj_papyrus_friendc_113", "\\E2* Not with THAT&  attitude!/%%")
	ds_map_add(global.text_data_en, "obj_papyrus_friendc_118", "\\E3FRISK..^1.&TAKE A HIKE./")
	ds_map_add(global.text_data_en, "obj_papyrus_friendc_119", "\\E0HIKING IS GOOD&FOR YOUR BONES./%%")
	ds_map_add(global.text_data_en, "obj_papyrus_guide_82", "WELCOME TO SCENIC&MY HOUSE!/")
	ds_map_add(global.text_data_en, "obj_papyrus_guide_83", "ENJOY AND TAKE&YOUR TIME!!!/%%")
	ds_map_add(global.text_data_en, "obj_papyrus_guide_84", "WOW!&BEING A GOOD HOST&IS A REAL WORKOUT!/%%")
	ds_map_add(global.text_data_en, "obj_papyrus_guide_88", "\\E3I CAN'T FEEL MY&LEGS.../")
	ds_map_add(global.text_data_en, "obj_papyrus_guide_89", "\\E0THAT MUST MEAN&I'M BEING A&GREAT HOST!!!/%%")
	ds_map_add(global.text_data_en, "obj_papyrus_hisroom_86", "TAKE A LOOK&AROUND!/%%")
	ds_map_add(global.text_data_en, "obj_papyrus_houseoutside_109", "SO YOU CAME&BACK TO HAVE A&DATE WITH ME!/")
	ds_map_add(global.text_data_en, "obj_papyrus_houseoutside_110", "SO YOU CAME&BACK TO SEE ME!/")
	ds_map_add(global.text_data_en, "obj_papyrus_houseoutside_111", "\\E3YOU MUST BE&REALLY SERIOUS&ABOUT THIS.../")
	ds_map_add(global.text_data_en, "obj_papyrus_houseoutside_112", "\\E2I'LL HAVE TO TAKE&YOU SOMEPLACE&REALLY SPECIAL.../")
	ds_map_add(global.text_data_en, "obj_papyrus_houseoutside_113", "\\E0A PLACE I LIKE&TO SPEND A LOT&OF TIME!!!/%%")
	ds_map_add(global.text_data_en, "obj_papyrus_houseoutside_303", "MY HOUSE!!!/%%")
	ds_map_add(global.text_data_en, "obj_papyrus_room_119", "MY BROTHER STARTED&A SOCK COLLECTION&RECENTLY./")
	ds_map_add(global.text_data_en, "obj_papyrus_room_120", "\\E3HOW SADDENING.../")
	ds_map_add(global.text_data_en, "obj_papyrus_room_121", "\\E0SOMETIMES I WONDER&WHAT HE WOULD&DO.../")
	ds_map_add(global.text_data_en, "obj_papyrus_room_122", "WITHOUT SUCH A&COOL GUY TAKING&CARE OF HIM???/")
	ds_map_add(global.text_data_en, "obj_papyrus_room_123", "NYEH HEH HEH!/%%")
	ds_map_add(global.text_data_en, "obj_papyrus_room_129", "SO^1, YOU WANT&A HINT^1, HUH?/")
	ds_map_add(global.text_data_en, "obj_papyrus_room_130", "\\E3WELL..^1.&I'VE BEEN LOOKING&AT THE PUZZLE.../")
	ds_map_add(global.text_data_en, "obj_papyrus_room_131", "AND I THINK THE&SOLUTION.../")
	ds_map_add(global.text_data_en, "obj_papyrus_room_132", "\\E0IS TO TURN ALL&OF THE X'S INTO&O'S!/")
	ds_map_add(global.text_data_en, "obj_papyrus_room_133", "YOU SHOULD TRY&THAT!/")
	ds_map_add(global.text_data_en, "obj_papyrus_room_134", "ASK AGAIN FOR&MORE GREAT HINTS!/%%")
	ds_map_add(global.text_data_en, "obj_papyrus_room_135", "\\WMAYBE SOLVING&THE \\YLAST PUZZLE\\W &AGAIN WILL HELP./")
	ds_map_add(global.text_data_en, "obj_papyrus_room_136", "ASK AGAIN FOR&MORE GREAT HINTS!!/%%")
	ds_map_add(global.text_data_en, "obj_papyrus_room_157", "Error, Pepibs./%%")
	ds_map_add(global.text_data_en, "obj_papyrus_room_161", "I ASKED UNDYNE&IF WE COULD&ALL HANG.../")
	ds_map_add(global.text_data_en, "obj_papyrus_room_162", "\\E3BUT SHE SAID&SOMETHING WEIRD./")
	ds_map_add(global.text_data_en, "obj_papyrus_room_163", "\\E0SHE SAID SHE&WON'T HANG OUT&WITH A MURDERER./")
	ds_map_add(global.text_data_en, "obj_papyrus_room_164", "\\E3BUT..^1. HMM.../")
	ds_map_add(global.text_data_en, "obj_papyrus_room_165", "\\E5I DON'T REMEMBER&MURDERERING&ANYONE.../")
	ds_map_add(global.text_data_en, "obj_papyrus_room_166", "\\E3THOUGH I AM A&PRETTY BRUTAL&KIND OF GUY./")
	ds_map_add(global.text_data_en, "obj_papyrus_room_167", "\\E3THEN SHE SAID&SOMETHING ABOUT&WANTING TO.../")
	ds_map_add(global.text_data_en, "obj_papyrus_room_168", "\\E1FIGHT SOME KIND&OF BAD GUY./")
	ds_map_add(global.text_data_en, "obj_papyrus_room_169", "\\E3BUT SHE SEEMS&VERY..^1. WEAK&FROM SOMETHING./")
	ds_map_add(global.text_data_en, "obj_papyrus_room_170", "\\E5SHE STEPPED OUT&AND COLLAPSED IN&THE DOORWAY./")
	ds_map_add(global.text_data_en, "obj_papyrus_room_171", "\\E0SO I'M JUST&WAITING HERE IN&CASE SHE NEEDS ME./")
	ds_map_add(global.text_data_en, "obj_papyrus_room_172", "\\E3SINCE SHE HATES&BEING WAITED ON&HAND AND FOOT./")
	ds_map_add(global.text_data_en, "obj_papyrus_room_173", "\\E5I THINK WE'LL&HAVE TO HANG&OUT LATER.../%%")
	ds_map_add(global.text_data_en, "obj_papyrus_room_176", "HEY^1, I.../")
	ds_map_add(global.text_data_en, "obj_papyrus_room_177", "\\E5REALLY DOUBT&WE'RE GOING TO&BE ABLE TO HANG./")
	ds_map_add(global.text_data_en, "obj_papyrus_room_178", "\\E3IT'S HARD TO&IMAGINE^1, BUT.../")
	ds_map_add(global.text_data_en, "obj_papyrus_room_179", "\\E0YOU'LL HAVE TO&SPEND TIME WITH&NOT ME./%%")
	ds_map_add(global.text_data_en, "obj_papyrus_room_185", "HEY^1! READY TO&HANG OUT WITH&UNDYNE???/")
	ds_map_add(global.text_data_en, "obj_papyrus_room_186", "GREAT!!^1!&ME TOO!!!/")
	ds_map_add(global.text_data_en, "obj_papyrus_room_187", "\\E3HOWEVER^1, SHE'S&NOT HERE./")
	ds_map_add(global.text_data_en, "obj_papyrus_room_188", "\\E0NORMALLY SHE&RETURNS AT ABOUT&THIS TIME./")
	ds_map_add(global.text_data_en, "obj_papyrus_room_189", "\\E3SHE'S LATE&THOUGH.../")
	ds_map_add(global.text_data_en, "obj_papyrus_room_190", "\\E3AND SHE'S NOT&ANSWERING HER&PHONE./")
	ds_map_add(global.text_data_en, "obj_papyrus_room_191", "\\E0HOW STRANGE./")
	ds_map_add(global.text_data_en, "obj_papyrus_room_192", "I'LL JUST WAIT&HERE UNTIL SHE&GETS HOME./")
	ds_map_add(global.text_data_en, "obj_papyrus_room_193", "I'LL CALL YOU&WHEN SHE GETS&HERE^1, OKAY!?/%%")
	ds_map_add(global.text_data_en, "obj_papyrus_room_196", "NOPE!^1!&SHE ISN'T HERE&YET!!!/%%")
	ds_map_add(global.text_data_en, "obj_papyrus_room_202", "UNDYNE'S HOME^1,&BUT SHE ISN'T&FEELING WELL./")
	ds_map_add(global.text_data_en, "obj_papyrus_room_203", "\\E5I DON'T THINK WE&CAN HANG OUT./")
	ds_map_add(global.text_data_en, "obj_papyrus_room_204", "\\E3IT SEEMS LIKE SHE&CAUGHT HEAT-STROKE&OR SOMETHING./")
	ds_map_add(global.text_data_en, "obj_papyrus_room_205", "SOMEONE MUST HAVE&BROUGHT HER&BACK HOME.../")
	ds_map_add(global.text_data_en, "obj_papyrus_room_206", "\\E5BECAUSE SHE CAN&BARELY MOVE./")
	ds_map_add(global.text_data_en, "obj_papyrus_room_207", "\\E0BUT SHE IS&MUTTERING ABOUT&DESTROYING YOU./")
	ds_map_add(global.text_data_en, "obj_papyrus_room_208", "SO THAT'S A GOOD&VITAL SIGN./")
	ds_map_add(global.text_data_en, "obj_papyrus_room_209", "\\E3IN ANY CASE^1, I&DON'T THINK WE&CAN HANG OUT./")
	ds_map_add(global.text_data_en, "obj_papyrus_room_210", "\\E0I'M JUST WAITING&HERE IN CASE&SHE NEEDS ME./")
	ds_map_add(global.text_data_en, "obj_papyrus_room_211", "SO^1, I GUESS YOU&CAN RUN ALONG./%%")
	ds_map_add(global.text_data_en, "obj_papyrus_room_214", "UNDYNE IS SICK^1,&SO SHE CAN'T&PLAY TODAY./%%")
	ds_map_add(global.text_data_en, "obj_papyrus_room_220", "HEY^1! READY TO&HANG OUT WITH&UNDYNE???/")
	ds_map_add(global.text_data_en, "obj_papyrus_room_221", "GREAT!!^1!&BECAUSE SHE'S NOT&EVEN HERE./")
	ds_map_add(global.text_data_en, "obj_papyrus_room_222", "BEING READY EARLY^1!&IT SHOWS HOW MUCH&YOU CARE!!/")
	ds_map_add(global.text_data_en, "obj_papyrus_room_223", "\\E3EVEN IF SHE'S&NOT HERE TO SEE&THAT./%%")
	ds_map_add(global.text_data_en, "obj_papyrus_room_226", "NOPE!^1!&SHE ISN'T HERE&YET!!!/%%")
	ds_map_add(global.text_data_en, "obj_papyrus_room_244", "HELLO^1, MY FRIEND./")
	ds_map_add(global.text_data_en, "obj_papyrus_room_245", "WHAT BRINGS YOU&TO PAPYRUS TOWN?/%%")
	ds_map_add(global.text_data_en, "obj_papyrus_room_248", "HELLO..^1.&H-HOW ARE YOU^1,&MY F-FRIEND?/")
	ds_map_add(global.text_data_en, "obj_papyrus_room_249", "\\E5I'M FEELING VERY&NOT SUSPICIOUS&TODAY./%%")
	ds_map_add(global.text_data_en, "obj_papyrus_room_253", "HEY^1, I HAVE A&GREAT IDEA./")
	ds_map_add(global.text_data_en, "obj_papyrus_room_254", "LET'S HANG OUT&WITH UNDYNE&LATER./")
	ds_map_add(global.text_data_en, "obj_papyrus_room_255", "I'LL CALL YOU&WHEN I'M READY!/%%")
	ds_map_add(global.text_data_en, "obj_papyrus_room_259", "I'M SO GLAD YOU&AND UNDYNE ARE&FRIENDS NOW./")
	ds_map_add(global.text_data_en, "obj_papyrus_room_260", "YOU TWO GO&TOGETHER LIKE&PASTA AND BURNING!/%%")
	ds_map_add(global.text_data_en, "obj_papyrus_room_263", "OH^1, HEY.../")
	ds_map_add(global.text_data_en, "obj_papyrus_room_264", "\\E3YOU'RE LEAVING&SOON^1, RIGHT!?/")
	ds_map_add(global.text_data_en, "obj_papyrus_room_265", "\\E1DON'T COME HERE&TO SAY GOODBYE!!/")
	ds_map_add(global.text_data_en, "obj_papyrus_room_266", "\\E3GOODBYES AREN'T&ALLOWED IN MY&TOWN./")
	ds_map_add(global.text_data_en, "obj_papyrus_room_267", "\\E0JUST SEE-YOU-&LATERS./%%")
	ds_map_add(global.text_data_en, "obj_papyrus_room_272", "IT SEEMS UNDYNE&HAS SOMETHING&FOR YOU./")
