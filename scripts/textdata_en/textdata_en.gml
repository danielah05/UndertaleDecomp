	ds_map_add(global.text_data_en, "obj_sansb_body_2143", "\\E5and then^1, you'll&finally quit./%%")
	ds_map_add(global.text_data_en, "obj_sansb_body_2161", "\\E5i know your type./")
	ds_map_add(global.text_data_en, "obj_sansb_body_2162", "\\E1you're^1, uh^1, very&determined^1, aren't&you?/")
	ds_map_add(global.text_data_en, "obj_sansb_body_2163", "\\E4you'll never give&up^1, even if&there's^1, uh.../")
	ds_map_add(global.text_data_en, "obj_sansb_body_2164", "\\E3absolutely NO&benefit to&persevering&whatsoever./")
	ds_map_add(global.text_data_en, "obj_sansb_body_2165", "\\E1if i can make&that clear./")
	ds_map_add(global.text_data_en, "obj_sansb_body_2166", "\\E4no matter what^1,&you'll just keep&going./")
	ds_map_add(global.text_data_en, "obj_sansb_body_2167", "\\E9not out of any&desire for good&or evil.../")
	ds_map_add(global.text_data_en, "obj_sansb_body_2168", "\\E3but just because&you think you&can./")
	ds_map_add(global.text_data_en, "obj_sansb_body_2169", "\\E1and because you&\"can\".../")
	ds_map_add(global.text_data_en, "obj_sansb_body_2170", "\\E9... you \"have to.\"/%%")
	ds_map_add(global.text_data_en, "obj_sansb_body_2188", "\\E9but now^1, you've&reached the end./")
	ds_map_add(global.text_data_en, "obj_sansb_body_2189", "\\E4there is nothing&left for you now./")
	ds_map_add(global.text_data_en, "obj_sansb_body_2190", "\\E1so^1, uh^1, in my&personal opinion.../")
	ds_map_add(global.text_data_en, "obj_sansb_body_2191", "\\E3the most&\"determined\"&thing you can&do here?/")
	ds_map_add(global.text_data_en, "obj_sansb_body_2192", "\\E1is to^1, uh^1,&completely give&up./")
	ds_map_add(global.text_data_en, "obj_sansb_body_2193", "\\E3and..^1. (yawn)&do literally&anything else./%%")
	ds_map_add(global.text_data_en, "obj_sansb_body_2344", "heh^1, didja&really think you&would be able to")
	ds_map_add(global.text_data_en, "obj_sansb_body_2495", ".../")
	ds_map_add(global.text_data_en, "obj_sansb_body_2496", ".../")
	ds_map_add(global.text_data_en, "obj_sansb_body_2497", ".../")
	ds_map_add(global.text_data_en, "obj_sansb_body_2498", "so.../")
	ds_map_add(global.text_data_en, "obj_sansb_body_2499", "guess that's it^1,&huh?/")
	ds_map_add(global.text_data_en, "obj_sansb_body_2500", ".../")
	ds_map_add(global.text_data_en, "obj_sansb_body_2501", "just.../")
	ds_map_add(global.text_data_en, "obj_sansb_body_2502", "don't say i&didn't warn you./%%")
	ds_map_add(global.text_data_en, "obj_sansb_body_2539", "welp./")
	ds_map_add(global.text_data_en, "obj_sansb_body_2540", "i'm going to&grillby's./%%")
	ds_map_add(global.text_data_en, "obj_sansb_body_2574", "papyrus^1, do you&want anything?/%%")
	ds_map_add(global.text_data_en, "obj_sansbyetrigger_124", "* actually^1, hey..^1.&* hate to bother ya^1, but&  can you do me a favor?/")
	ds_map_add(global.text_data_en, "obj_sansbyetrigger_125", "* i was thinking..^1.&* my brother's been&  kind of down lately.../")
	ds_map_add(global.text_data_en, "obj_sansbyetrigger_126", "* he's never seen a human&  before^1. and seeing you&  might just make his day./")
	ds_map_add(global.text_data_en, "obj_sansbyetrigger_127", "* don't worry^1, he's not&  dangerous./")
	ds_map_add(global.text_data_en, "obj_sansbyetrigger_128", "\\E1* even if he tries to be./")
	ds_map_add(global.text_data_en, "obj_sansbyetrigger_129", "\\E0* thanks a million^1.&* i'll be up ahead./%%")
