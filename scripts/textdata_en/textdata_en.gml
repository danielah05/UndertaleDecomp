	ds_map_add(global.text_data_en, "obj_sansb_960", "* Felt like a turning point.")
	ds_map_add(global.text_data_en, "obj_sansb_962", "* You felt your sins crawling&  on your back.")
	ds_map_add(global.text_data_en, "obj_sansb_963", "* You felt your sins weighing&  on your neck.")
