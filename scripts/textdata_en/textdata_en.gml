	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_0", "* (It's 25G for 5 Dog Coins.)&* (Exchange?)&         Yes         No \\C")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_1", "* (You received a pittance&  of Coine.)/%")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_2", "* (You seem to have collected&  all the Collectible Coins in&  the vicinity.)/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_3", "* (What will you do now...)/%")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_4", "* (You lack the funds to buy&  basic essentials^1, such as&  money.)/%")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_5", "* (You realized that the Coins&  are just G, squished by a&  Collectible Coin machine.)/%")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_6", "* if you win anything,&  take a look at it&  and talk to me./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_7", "\\E1* someone's gotta roast&  you for all your&  hard work./%")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_8", "* well^1?&* got anything?/%")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_9", "* speaking of tall^1, my&  brother increased the&  height of the sink./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_10", "\\E1* the original height just&  wasn't challenging&  enough anymore./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_11", "\\E2* yep^1. he's not someone&  that takes shortcuts./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_12", "\\E0* ... guess he wanted to&  show they aren't&  necessary./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_13", "\\E1* in his own kind of^1,&  special^1, inconveniencing&  way./%")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_14", "* fried dog food^1?&* nah^1, don't ya know&  fried food's unhealthy?/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_15", "\\E2* don't trust anyone who&  tries to sell you that&  stuff./%")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_16", "* fast food^1?&* from that hotland joint?/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_17", "\\E1* sure^1, i like a good burg^1,&  but the restaurant one&  over's where it's at./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_18", "\\E0* got a nice atmosphere^1.&* done some comedy shows&  there on the stage./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_19", "\\E1* the king even came once.&* i said one pun and he&  just started going wild./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_20", "\\E3* so i just stood there^1,&  waiting for him to stop&  laughing.../")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_21", "\\E1* but he didn't^1, so after&  a while they just closed&  the curtains on me./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_22", "\\E2* probably my best show./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_23", "\\E0* anyway^1.&* i'll treat ya some time&  if you're in the area./%")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_24", "* hey^1, a game list^1.&* pretty sure i know&  who made that./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_25", "* i was asking her if she&  had any old game&  recs for a friend./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_26", "\\E1* like maybe some easy&  ones about farming^1,&  or snails^1, or something./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_27", "\\E0* but i forgot that she's&  the kind of person&  who only, uh.../")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_28", "\\E1* recommends whatever it&  is she's currently&  obsessed with./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_29", "\\E0* so she just told me&  the same rpg as always./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_30", "\\E3* heh./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_31", "\\E2* told her i'd try it&  if it ever came out on&  gamerbox./%")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_32", "* skin cream^1?&* no thanks./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_33", "\\E1* pretty sure that's my&  brother's stuff anyway./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_34", "\\E0* he loves skin care^1.&* if you couldn't tell&  just by looking./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_35", "\\E1* never had a pimple&  ever since he started^1,&  he says./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_36", "\\E0* i'll intimidate the&  humans with my spotless&  face^1, he says./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_37", "\\E1* sans^1, don't bother me&  when i'm in the hyper-&  bolic cucumber chamber,/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_38", "\\E0* he says^1, but i couldn't&  tell you what it&  means./%")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_39", "* what^1, you wanna see&  me put on that hat?/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_40", "\\E3* nah^1. two reasons./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_41", "\\E1* first^1, i just combed./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_42", "\\E2* and second^1, do i look&  like the kind of guy&  to wear a hat?/%")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_43", "* a ticket to mtt's live&  gaming show^1, huh^1?&* nice./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_44", "\\E1* those are always a hoot./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_45", "\\E0* he only plays games with&  mods that make him&  the main character./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_46", "\\E1* but^1, uh^1, all the&  animations stay the&  same^1, so.../")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_47", "\\E2* kinda funny when he's&  supposed to be^1, say^1, a&  long-tongued lizard?/%")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_48", "* what's that^1, some&  kinda camera?/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_49", "\\E2* yeah^1, papyrus's been&  really into taking&  'selfies' lately./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_50", "\\E0* he just can't wait&  for the^1, uh^1, 'results&  of his training.'/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_51", "\\E1* so he's been editing&  them to^1, uh^1, make&  his muscles look bigger./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_52", "\\E2* it's not false^1, he&  says^1. it's just a&  glimpse of the future./%")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_53", "* a gamerbox^1.&* wow./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_54", "\\E1* can't believe you got&  one in real life./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_55", "\\E0* even if there's some&  kinda exclusive game&  for it or something.../")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_56", "\\E2* couldn't you just watch&  a video instead of&  playing it yourself?/%")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_57", "* wow^1. the 'perfect&  combo of monsters&  and energy.'/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_58", "* i know someone who&  was always downing&  that stuff./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_59", "* she was saving the&  cans as scrap metal&  for something big.../")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_60", "* it's 'flavored after&  a famous cherry.'/%")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_61", "* wow^1, look at all&  that fuzz./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_62", "\\E1* looks like our house&  after a certain&  dog comes by./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_63", "\\E3* then papyrus gets&  out his broom^1, stomps&  his feet^1, and goes.../")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_64", "\\E2* sans^1, you have&  got to stop shedding!/%")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_65", "* wow^1, that fluffy&  stuff is really&  coming down./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_66", "\\E3* when it snows^1, papyrus&  always puts his face&  to the window.../")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_67", "\\E0* and goes^1, sans^1, look^1,&  it's snowing^1! isn't it&  beautiful!?/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_68", "\\E0* and i say.../")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_69", "\\E1* you know it snows&  almost every day^1,&  right?/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_70", "\\E3* but he's already out&  the door making a&  snow angel./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_71", "\\E1* and then^1, uh^1, fixing&  up its muscles to&  make them bigger./%")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_72", "* woah^1, it's looking&  like a blizzard&  in here./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_73", "* i've only seen it&  snow as bad as&  this once./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_74", "\\E3* you could barely see&  your bones in front&  of your face./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_75", "\\E0* but even so^1, the&  king was out there&  in red and white,/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_76", "\\E2* putting down the&  presents for everyone./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_77", "\\E3* papyrus^1, the yellow&  kid^1, the antlered girl&  and her big sis.../")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_78", "\\E2* he even had a little&  something for me./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_79", "\\E1* even though he didn't&  really know me./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_80", "\\E3* .../")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_81", "\\E0* you're gonna have to&  meet him^1, huh./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_82", "\\E1* bet that's a lot&  of pressure./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_83", "\\E0* so^1, i guess if it&  makes anything&  easier.../")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_84", "\\E3* don't think of him&  as the king./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_85", "\\E2* just think of him as&  the guy who gave me&  some socks./%")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_86", "* those were the days&  before i took sock&  collecting seriously./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_87", "\\E3* hate to say it^1,&  but.../")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_88", "\\E2* i was a noob back&  then./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_89", "\\E1* i knew absolutely&  nothing about the&  competitive scene./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_90", "\\E2* but things changed^1.&* things changed^1, kid./%")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_91", "* heya./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_92", "\\E2* how about a game of&  dog poker?/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_93", "\\E0* it's only 10G.& &  Yes         No \\C")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_94", "* too bad poker wasn't&  in the cards./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_95", "\\E1* tell you what though^1.&* if you win any prizes.../")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_96", "\\E0* take a look at 'em&  and talk to me./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_97", "\\E2* someone's gotta roast&  you for all your&  hard work./%")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_98", "\\E0* whoops^1, looks like&  you can't reach the&  table./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_99", "\\E1* sorry^1, i keep forgetting&  how much taller i&  am than everyone./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_100", "\\E0* guess we can't play./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_101", "\\E2* i'll try putting up a&  sign next time./%")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_102", "\\E2* what^1, intimidated by&  the height of my table?/%")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_103", "* Got anything to set&  my peepers on!?/%")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_104", "* What^1, what^1, WHAT!^1?&* Check something you want&  my thoughts on!/%")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_105", "* This is one of the highest&  paying jobs I've ever had^1,&  mew!/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_106", "* Er^1, I don't technically&  have a salary^1, on&  account of.../")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_107", "* Just finding this place&  and randomly deciding&  I work here./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_108", "* BUT I've been replicating&  this canine residue I found^1,&  and I'm gonna sell it ALL!/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_109", "* HAHA^1! Then I'll be ROLLING&  in FLAKES^1! ROLLING around on&  my back with my paws up!/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_110", "* ME and my ANIMATRONIC&  VOICEBOX both LAUGHING&  IN SYNC!/%")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_111", "* Nice sign huh!?/%")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_112", "* Nice sign huh!?&* I found it myself!/%")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_113", "* Hmm..^1.&* A humanoid statue...?/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_114", "* Hey^1, hey^1, HEY^1!&* I know this nice ghost that&  could REALLY use a body!/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_115", "* Just replace the face^1,&  and you've got a WINNER!/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_116", "* Just THINK about how many&  KNIVES it could hold in&  its arms^1, mew!/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_117", "* KNIVES!^1!&* Physical^1, CORPOREAL KNIVES!/%")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_118", "* Fast food^1?&* Is that from that&  WORTHLESS MTT store?/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_119", "* Pass^1. Pass^1! PASS!/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_120", "* The guy working there is&  SUCH a creep^1, it's not&  even funny!/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_121", "* He just kept STARING^1!&* Like you've never seen a&  pretty girl.../")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_122", "* ... turn her head around&  270 degrees?/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_123", "* While^1, cutting all the&  pictures of your boss^1,&  out of the menu?/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_124", "* And replacing them^1, with&  pictures^1, of herself?/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_125", "* It's absolutely normal for&  girls to do that is what&  I'm saying./%")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_126", "* Good games^1? Yeah^1, you mean&  Mew Mew 2 Super Cutey&  Darkness Dress Up!?/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_127", "* Someone made up a game&  where you dress ME up^1.&* Sooooo cute^1, mew!/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_128", "* And DON'T listen to any&  fool who says it's&  'non-canon.'/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_129", "* That SAME person^1, would say&  I'm 'degenerating Mew&  Mew's character,'/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_130", "* JUST for how I walk^1,&  how I talk^1, or.../")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_131", "* Even just innocently&  brandishing some&  knives!/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_132", "* Well^1, MEWS FLASH^1!&* I'M Mew Mew now^1, you&  futile fools!/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_133", "* You see me blowing a big&  cloud of smoke out of&  my mouth at an event?/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_134", "* That's canon!/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_135", "* And because my body is&  shoddily made and emits&  smoke constantly!/%")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_136", "* Mmmm^1, Beauty Creams^1!&* I love that stuff^1, nya!/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_137", "* Of course^1, I can't taste&  anything yet^1, but.../")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_138", "* The feeling of my&  teeth grinding it up is&  great!/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_139", "* But don't let the MTT&  branding fool you^1.&* Oh^1, don't^1! Don't^1! DON'T!/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_140", "* When I went to the MTT&  burger place^1, ONCE AGAIN^1,&  the FREAK working there.../")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_141", "* Just STARED at me when I&  asked him to make ICE&  CREAM out of it for me!/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_142", "* Like^1, HELLO^1, is your head&  screwed on right^1?&* NO?/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_143", "* And I'M BAD for TRYING&  to HELP him re-screw&  his head on!^1? WHAT?/%")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_144", "* Wouldn't Undyne look&  GREAT in a baseball&  cap^1, mew?/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_145", "* Her hair'd go PERFECTLY&  through the hole in&  the back!/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_146", "* Wait^1.&* Wait^1!&* WAIT!/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_147", "* That's a BACKWARDS cap^1!&* No^1, that wouldn't work^1!&* If she wore it backwards.../")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_148", "* You'd have to cut a hole&  above the bill for her&  hair to come out of!/%")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_149", "* That ticket^1? N-no^1, I haven't&  seen it before!/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_150", "* What^1? No^1, I don't know&  if it's originally&  Blooky's^1! No idea!/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_151", "* Just^1, don't you think it's&  a bad influence on Blooky if&  they watch that weird robot!?/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_152", "* He's just so..^1.&* Inappropriately..^1.&* Corporeal!/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_153", "* What!^1? I-it's not my fault&  if I can't face them since I&  left to try to find a body.../")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_154", "* E-even if it means^1, not&  letting them find out^1,&  I've been their neighbor.../")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_155", "* Look^1! It's just something&  you can't understand^1, mew!/%")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_156", "* Shucks^1, is that a CAMERA!^1?&* Like^1, want to take a&  selfie with me^1, mew?/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_157", "* TOO BAD!^1! You gotta PAY for&  photos of me^1! I'm trying&  to build up my BRAND!/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_158", "* How else will I afford PINK&  DRESSES for all of my&  BULLETS?/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_159", "* Post your OWN selfie^1! I'll&  LIKE and SHARE it^1! Haha^1!&* Serves you right!/%")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_160", "* That white box^1? Pretty sure&  this body interfaces&  with it somehow!/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_161", "* There's some Mew Mew game&  on there^1, where if you&  press the controller.../")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_162", "* It makes my body's voice&  box speak in response&  to what's onscreen!/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_163", "* Alphys was using it trying&  to find this body right&  when I^1, er^1, took it over!/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_164", "* I could hear her footsteps&  approaching as she kept&  tapping the X button.../")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_165", "* I crouched in the corner^1,&  trying to cover my mouth&  and voicebox^1, but.../")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_166", "* My body kept yelling&  'Right here^1! I'm right&  here, mew!'/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_167", "* Finally^1, when she was&  just about to turn the&  corner and see me.../")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_168", "* I stood up and shouted&  with my own voice./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_169", "\\W* '\\RI'M RIGHT HERE^1, MEW!\\W'/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_170", "* She fainted./")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_171", "* And that's my story^1, mew!/%")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_172", "* Energy drinks^1? What^1?/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_173", "* The only kind of energy&  drink I need is&  ELECTRICITY!/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_174", "* Because if I don't charge&  my voicebox.../")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_175", "* Demonic^1, low-pitched voice&  clips emanate from inside&  of me!/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_176", "* 'Feeeeeeed meee, mewwwww!'/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_177", "* Even SO^1, Burger Freak&  REFUSES to make me a&  battery smoothie.../")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_178", "* Open me up^1, and pour&  it inside!/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_179", "* What is WRONG with him^1?&* Don't make me go to&  Alphys^1, mew!/%")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_180", "* Shucks^1, look at all that&  dog fuzz^1, mew^1!&* Looks kind of like.../")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_181", "* BULLETS!/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_182", "* Remember when I fired all&  of those cotton balls at&  you^1, mew?/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_183", "* Then fired all of my own&  bullets?/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_184", "* So nostalgic^1! When are we&  gonna do that AGAIN!?/%")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_185", "* Uwa^1, there's gotta be more&  than 500 particles flying&  around right nyaow!/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_186", "* Ahh^1, reminds me of back when&  I'd get cut and my cotton&  would fly out everywhere.../")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_187", "* Undyne^1! I'm still jointed and&  easy to put back together^1!&* You don't have to hold back!/")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_188", "* ... I want to say to her^1,&  but.../")
	ds_map_add(global.text_data_en, "SCR_TEXT_xbox_189", "* ... if I fuse with this body^1,&  I might really get hurt if&  she uses it for training.../")
