	ds_map_add(global.text_data_en, "obj_undyne_ex_867", "Heheheh.../")
	ds_map_add(global.text_data_en, "obj_undyne_ex_868", "\\E2If you..^1./")
	ds_map_add(global.text_data_en, "obj_undyne_ex_869", "If you think I'm&gonna give up hope^1,&you're wrong./")
