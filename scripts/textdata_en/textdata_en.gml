	ds_map_add(global.text_data_en, "SCR_TEXT_1891", "* Well^1, talk to me&  again if you&  need anything./%%")
	ds_map_add(global.text_data_en, "SCR_TEXT_1895", "\\E1* What^1?&* This..^1. this IS your&  home now./")
	ds_map_add(global.text_data_en, "SCR_TEXT_1896", "* Um..^1. would you like&  to hear about this&  book I am reading?/")
	ds_map_add(global.text_data_en, "SCR_TEXT_1897", "\\E0* Oh^1, hello!/")
	ds_map_add(global.text_data_en, "SCR_TEXT_1898", "* Did you want to&  hear about the&  book I am reading?/")
	ds_map_add(global.text_data_en, "SCR_TEXT_1899", "\\E0* It is called&  \"72 Uses for Snails.\"/")
