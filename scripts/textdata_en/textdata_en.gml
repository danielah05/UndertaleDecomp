	ds_map_add(global.text_data_en, "obj_sansbyetrigger_133", "\\E1* my brother'd really&  like to see a human.../")
	ds_map_add(global.text_data_en, "obj_sansbyetrigger_134", "\\E2* so^1, y'know^1, it'd&  really help me out.../")
	ds_map_add(global.text_data_en, "obj_sansbyetrigger_135", "\\E1* if you kept pretending&  to be one./%%")
	ds_map_add(global.text_data_en, "obj_sanscheckpoint_98", "* It's some sort of checkpoint&  or sentry-station./")
	ds_map_add(global.text_data_en, "obj_sanscheckpoint_99", "* But there are bottles of&  ketchup^1, mustard^1, and relish&  sitting inside.../%%")
