	ds_map_add(global.text_data_en, "obj_sansb_1084", "it's a beautiful&day outside./")
	ds_map_add(global.text_data_en, "obj_sansb_1085", "birds are singing^1,%%")
	ds_map_add(global.text_data_en, "obj_sansb_1089", "ready?/%%")
	ds_map_add(global.text_data_en, "obj_sansb_1152", "Should&be&burning&in hell./%%")
	ds_map_add(global.text_data_en, "obj_sansb_body_1446", "\\E1huh./")
	ds_map_add(global.text_data_en, "obj_sansb_body_1447", "\\M1always wondered why&people never use&their strongest&attack first./%%")
	ds_map_add(global.text_data_en, "obj_sansb_body_1450", "\\E3anyway^1, as i was&saying^1, it's a&nice day out./")
	ds_map_add(global.text_data_en, "obj_sansb_body_1451", "\\M1why not relax and&take a load off?/%%")
	ds_map_add(global.text_data_en, "obj_sansb_body_1455", "here we go./%%")
	ds_map_add(global.text_data_en, "obj_sansb_body_1468", "* You feel like you're going to&  have a bad time.")
	ds_map_add(global.text_data_en, "obj_sansb_body_2091", "huff..^1. puff.../")
	ds_map_add(global.text_data_en, "obj_sansb_body_2092", "all right^1.&that's it./")
	ds_map_add(global.text_data_en, "obj_sansb_body_2093", "\\Xi\\M1t's time for my\\R &special attack\\X./")
	ds_map_add(global.text_data_en, "obj_sansb_body_2094", "\\E3are you ready?/")
	ds_map_add(global.text_data_en, "obj_sansb_body_2095", "\\E4here goes nothing./%%")
	ds_map_add(global.text_data_en, "obj_sansb_body_2111", "\\E1yep./")
	ds_map_add(global.text_data_en, "obj_sansb_body_2112", "\\E1that's right./")
	ds_map_add(global.text_data_en, "obj_sansb_body_2113", "\\E3it's literally&nothing./")
	ds_map_add(global.text_data_en, "obj_sansb_body_2114", "\\E1and it's not gonna&be anything^1,&either./")
	ds_map_add(global.text_data_en, "obj_sansb_body_2115", "\\E4heh heh heh..^1.&ya get it?/")
	ds_map_add(global.text_data_en, "obj_sansb_body_2116", "\\E1i know i can't&beat you./")
	ds_map_add(global.text_data_en, "obj_sansb_body_2117", "\\E4one of your turns.../")
	ds_map_add(global.text_data_en, "obj_sansb_body_2118", "\\E9you're just gonna&kill me./")
	ds_map_add(global.text_data_en, "obj_sansb_body_2119", "\\E1so^1, uh./")
	ds_map_add(global.text_data_en, "obj_sansb_body_2120", "\\E4i've decided.../")
