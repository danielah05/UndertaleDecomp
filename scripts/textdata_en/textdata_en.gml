	ds_map_add(global.text_data_en, "obj_readable_room5_6", "* (That's completely untrue.^1)&* (It's just the kind of&  feeling it has.)/%")
	ds_map_add(global.text_data_en, "obj_readable_room5_7", "* (It's some kind of fried food&  dog effigy.)/%")
	ds_map_add(global.text_data_en, "obj_readable_room5_8", "* (It's a fast food box.^1)&* (Inside^1, dressed with ketchup&  and buns...)/")
	ds_map_add(global.text_data_en, "obj_readable_room5_9", "* (Are some game controllers.)/")
	ds_map_add(global.text_data_en, "obj_readable_room5_10", "* (There are bite marks.^1)&* (Seems the dog didn't get&  what it was buying.)/%")
	ds_map_add(global.text_data_en, "obj_readable_room5_11", "* (It's a fast food container&  with controllers inside.^1)&* (No mustard.)/%")
